    jQuery(document).ready(function($){
        $('.toggle-notifications-js').prepend($('.um-notification-live-count'));

        $(document).on('click', '.toggle-notifications-js', function(e){
            e.preventDefault();
            if ( $('.um-notification-live-feed').is(':hidden') ) {
                notification_responsive();
                $('.um-notification-live-feed').show("fast");
            } else {
                $('.um-notification-live-feed').hide("fast");
