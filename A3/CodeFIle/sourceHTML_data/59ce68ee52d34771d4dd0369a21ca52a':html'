<script type="text/javascript">

	(function($) {
	var xmlhttp = new XMLHttpRequest();
    var url = "https://www.btcturk.com/api/ticker";
    xmlhttp.onreadystatechange = function() {
      if (this.readyState == 4  &&  this.status == 200) {
        var data = JSON.parse(this.responseText);
        parseJson(data);
      }		
    };
    xmlhttp.open("GET", url, true);
    xmlhttp.send();
	
    function parseJson(data) {
	 

	var builddata = function () {
		var source = [];
		var items = [];
		var i;
		// build hierarchical source.
		for (i = 0; i < 5; i++) {
			var item = data[i];
			var label = item['numeratorsymbol'];
			var id = item['pair'];
			var price = item['last'];		
				items[i] = { id: id, label: label, price: price };
				source[i] = items[i];
		}
		return source;
	}

	var source = builddata();
	var buildUL = function (parent, items) {
		$.each(items, function () {
			if (this.label) {
            // create LI element and append it to the parent element.
            var li = $('<li class="coinprice"><a href="https://www.btcturk.com/?utm_source=uzmancoin&utm_medium=banner&utm_campaign=fiyat-butonu-desktop" target="_blank" data-wpel-link="external" rel="nofollow noreferrer"><strong>'+this.label+': </strong>'+this.price+'<span style="font-family:sans-serif !important;"> ₺ </span></a></li>');
            li.appendTo(parent);            
        }
    });
	}
	
	var ul = $('<ul id="btcprices"></ul>');
	ul.appendTo("#menubtc");
	buildUL(ul, source);
	
	var buildUL2 = function (parent, items) {
		$.each(items, function () {
			if (this.label) {
            // create LI element and append it to the parent element.
            var li = $('<li class="coinprice2"><a href="https://www.btcturk.com/?utm_source=uzmancoin&utm_medium=banner&utm_campaign=fiyat-butonu-mobile" target="_blank" data-wpel-link="external" rel="nofollow noreferrer"><strong>'+this.label+'</strong><br>'+this.price+'<span style="font-family:sans-serif !important;"> ₺ </span></a></li>');
            li.appendTo(parent); 
			return (this.label !== 'ETH');			
        }
    });
	}
	
	var ul2 = $('<ul id="btcprices2"></ul>');
	ul2.appendTo("#mobilebtc");
	buildUL2(ul2, source);
	
}		
})(jQuery);
	</script><script>
setTimeout (function() {Array.prototype.forEach.call (document.querySelectorAll (".ai-viewports"), function (element, index) {ai_insert_viewport (element);});}, 10);

</script>
