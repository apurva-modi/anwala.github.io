			<script>
function gct() {
    var m = new Date();
    var auxH= m.getUTCHours()+1;
    var dateString =
        ("0" + auxH).slice(-2) + ":" +
        ("0" + m.getUTCMinutes()).slice(-2) + ":" +
        ("0" + m.getUTCSeconds()).slice(-2);
    return dateString;
    
}

setInterval( function() {
    jQuery('#gct span').text(gct());
}, 1000);


</script>
