function wpfdr_15(){
jQuery(document).ready(function($){
$('body').on('click' , 'a' , function(e){
var $this=$(this),
linkID=$this.data('linkid'),
href=linkID ? $this.attr('href'):thirstyFunctions.isThirstyLink($this.attr('href')),
action_name='Affiliate Link',
page_slug='/cyber-security/alleged-sim-swap-crypto-thief-indicted-for-hacking-over-50-us-victims/',
home_url='https://www.prosyscom.tech';
if(! href||typeof ga!=='function')
return;
var is_uncloak=href.indexOf(home_url + '/' + thirsty_global_vars.link_prefix) < 0,
href_parts=href.split('/'),
href_last=href_parts[ href_parts.length - 1 ] ? href_parts[ href_parts.length - 1 ]:href_parts[ href_parts.length - 2 ],
link_text=$this.text(),
link_uri=linkID&&is_uncloak ? href:href.replace(home_url , ''),
link_slug=linkID&&is_uncloak ? href:href_last;
ga('send' , 'event' , {
eventCategory:action_name,
eventAction:link_uri,
eventLabel:page_slug,
transport:'beacon'
});
});
});
}</script>
<script>if(window.attachEvent){window.attachEvent('onload',wpfdr_14);}else if(window.addEventListener){window.addEventListener('load', wpfdr_14,false);}
function wpfdr_14(){
jQuery(document).ready(function(){
var wpfcWpfcAjaxCall=function(polls){
if(polls.length > 0){
poll_id=polls.last().attr('id').match(/\d+/)[0];
jQuery.ajax({
type: 'POST',
url: pollsL10n.ajax_url,
dataType:"json",
data:{"action": "wpfc_wppolls_ajax_request", "poll_id": poll_id, "nonce":"d33712b9e1"},
cache: false,
success: function(data){
if(data===true){
poll_result(poll_id);
}else if(data===false){
poll_booth(poll_id);
}
polls.length=polls.length - 1;
setTimeout(function(){
wpfcWpfcAjaxCall(polls);
}, 1000);
}});
}};
var polls=jQuery('div[id^=\"polls-\"][id$=\"-loading\"]');
wpfcWpfcAjaxCall(polls);
});
}</script>
