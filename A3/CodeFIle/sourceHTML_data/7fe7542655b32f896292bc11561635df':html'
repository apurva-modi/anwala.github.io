        var TNWCookieConsent = {
            NAME: 'tnw_consent_cookie',

            accept: function () {
                this.setCookie();
                this.hideDialog();
                return false;
            },

            hideDialog: function () {
                document.getElementsByTagName("html")[0].className = document.getElementsByTagName("html")[0].className.replace(/(?:^|\s)no-cookies(?!\S)/g, "");
            },

            init: function () {
                var cookie;
                var parts;
                var val;

                val = "; " + document.cookie;
                parts = val.split("; " + this.NAME + "=");

                if (parts.length == 1) {
                    this.showDialog();
                }
            },

            setCookie: function () {
                var exdate;
                var exdays;
                var val;

                exdays = 365;
                exdate = new Date();
                exdate.setDate(exdate.getDate() + exdays);
                val = escape("1") + ((exdays == null) ? "" : "; path=/; expires=" + exdate.toUTCString());
                document.cookie = this.NAME + "=" + val;
            },

            showDialog: function () {
                document.getElementsByTagName("html")[0].className = document.getElementsByTagName("html")[0].className += " no-cookies";
            }
        }

