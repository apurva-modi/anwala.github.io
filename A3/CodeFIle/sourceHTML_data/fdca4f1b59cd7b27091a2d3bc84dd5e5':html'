<script>
    window.ga=window.ga||function(){(ga.q=ga.q||[]).push(arguments)};ga.l=+new Date;

    // Helper
    var formatTimestampIso8601 = function(date) {
        var tzo = -date.getTimezoneOffset(),
            dif = tzo >= 0 ? '+' : '-';

        var pad = function(num) {
            var norm = Math.abs(Math.floor(num));
            return (norm < 10 ? '0' : '') + norm;
        };

        return date.getFullYear() +
            '-' + pad(date.getMonth()+1) +
            '-' + pad(date.getDate()) +
            'T' + pad(date.getHours()) +
            ':' + pad(date.getMinutes()) +
            ':' + pad(date.getSeconds()) +
            dif + pad(tzo / 60) +
            ':' + pad(tzo % 60);
    };

    var getSessionId = function() {
        // NOTE: we're using a randomized string that is sent with each pageview.
        //       The string changes with each pageview, but this doesn't matter
        //       because we're sending it to a session-scoped dimension.
        return new Date().getTime() + '-' + Math.random().toString(36).substring(5);
    };

    // Create tracker
    ga('create', {
        trackingId:     "UA-9526709-1",
        cookieDomain:   undefined,
        cookieExpires:  7200,
        userId:         undefined    });

    // Dimensions
            
        // User Type
        ga('set', 'dimension3', "not-logged");

        // Session ID
        ga('set', 'dimension4', getSessionId());
    
    // Anonymize IP
    ga('set', 'anonymizeIp', true);

    // Track pageviews
            ga('send', 'pageview', {
            page:       "\/user\/spaced-out-radio\/feb-15-19-the-crypto-guru-ronald-murphy",
            // Hit Timestamp
            dimension1: formatTimestampIso8601(new Date())
        });
    </script>

