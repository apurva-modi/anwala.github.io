}</style> <script>jQuery(document).ready(function () {
		var bitcoinQuotes = jQuery('.bitcoin-quotes');
		var bitcoinSellPrice = jQuery('#bitcoin-sell-price');
		var bitcoinBuyPrice = jQuery('#bitcoin-buy-price');

		jQuery.ajax({
			url: 'https://quote.coins.ph/v1/markets/BTC-PHP'
		}).done(function(response, status) {
			if (status === 'success') {
				var marketQuotes = response.market;
				bitcoinBuyPrice.text(Number(marketQuotes.ask).toLocaleString());
				bitcoinSellPrice.text(Number(marketQuotes.bid).toLocaleString());
			}
		});

		jQuery(document).on('scroll', function () {
			var scrollPosition = jQuery(document).scrollTop();
			var documentHeight = jQuery(document).height();
			var breakPoint = documentHeight * .1;
			if (scrollPosition > breakPoint) {
				bitcoinQuotes.fadeOut();
			} else {
				var isVisible = bitcoinQuotes.is(":visible");
				if (!isVisible) {
					bitcoinQuotes.fadeIn();
				}
			}
		});
	});</script> <div class="bitcoin-quotes"> <span class="bitcoin-quotes__title">coins.ph</span> <span class="bitcoin-quotes__price">Buy: <span id="bitcoin-buy-price">0</span> PHP</span> <span class="bitcoin-quotes__space">|</span> <span class="bitcoin-quotes__price">Sell: <span id="bitcoin-sell-price">0</span> PHP</span></div></div></div></div><div style="" class="vc_col-sm-12 wpb_column column_container  _ height-full"><div id="padding-3" class="mk-padding-divider hidden-sm  clearfix"></div><h1 id="fancy-title-4" class="mk-fancy-title  simple-style  color-single"> <span><h1 style="color: #ffffff; font-family: Lato,sans-serif;">Do more with your money</h1> </span></h1><div class="clearboth"></div><style>#fancy-title-4 {font-family: Arial, Helvetica, sans-serif !important}</style><div id="text-block-5" class="mk-text-block   homepage-subtitle"><h5 class="homepage-subtitle"></h5><div class="clearboth"></div></div><div id="mk-gradient-button-6" class="mk-gradient-button fullwidth-false btn-align-center custom-width-true"> <a href="https://app.coins.ph/welcome/signup" class="mk-button mk-button--dimension-flat mk-button--size-large mk-button--corner-full_rounded light-skin" target="_self"> <span class="text">GET STARTED</span> <i class="darker-background"></i></a></div><div class="wpb_raw_code wpb_content_element wpb_raw_html"><div class="wpb_wrapper"><div class="coins-icon-block"><ul><li><a href="https://app.coins.ph/welcome/signup"><img class="lazy lazy-hidden" src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" data-lazy-type="image" data-lazy-src="https://content.coins.ph/wp-content/uploads/2017/06/min-icon-desktop.png"><noscript><img src="https://content.coins.ph/wp-content/uploads/2017/06/min-icon-desktop.png"></noscript></a></li><li><a href="https://itunes.apple.com/us/app/coins-wallet/id972324049?mt=8"><img class="lazy lazy-hidden" src="data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7" data-lazy-type=