		var currentHour = new Date().getHours() + 1
		var calculatedMinutes = previousMinutes + (minutesOffset * currentHour)

		var selectedMetric = timeOptions[Math.floor(Math.random()*timeOptions.length)]
		var odometerValue = calculatedMinutes / selectedMetric.divideBy;
		var odometerType = selectedMetric.metric;

		if (['centuries', 'decades'].indexOf(selectedMetric.metric) === -1) {
			odometerValue = Math.floor(odometerValue).toString().replace(/B(?=(d{3})+(?!d))/g, ",");
		} else {
			odometerValue = odometerValue.toFixed(1).toString().replace(/B(?=(d{3})+(?!d))/g, ",");
		}

		var tagLine = '<span class="odometer-wrapper">Coins.ph has saved Filipinos more than ' + 
		              '<span class="odometer-block"><span class="odometer">'+odometerValue+'</span> ' + 
		              '<span class="odometer-metric">'+odometerType+'</span></span> ' +
		              'of standing in line since 2014</span>';

		tagLineSpace.html(tagLine);

		var odometerWrapper = jQuery('.odometer-wrapper');
		var odometer = jQuery('.odometer');
		var odometerMetric = jQuery('.odometer-metric');

		setTimeout(function () {
			odometerWrapper.css('visibility', 'visible');
			odometer.html(0);
			odometer.css('visibility', 'visible');
			odometerMetric.html(odometerType);

			var odometerInit = new Odometer({ el: jQuery('.odometer')[0], value: 0 });
			odometerInit.render();
			odometerInit.update(odometerValue);
                        odometerMetric.html(odometerType);
		}, 1000);
	}

	if (window.location.pathname === '/') {
		jQuery.ajax({ url: 'https://api.coins.asia/v3/minutes' }).done(function (res) {
			if (res.minutes && res.minutes > 0) {
                                var minutes = res.minutes;
                                var previousMinutes = res.previous_minutes;
                                try {
				    setTagLine(minutes, previousMinutes);
                                } catch (e) {
                                    // odometer lib not set up
                                }
			}
		});
	}
});

// Auth Check
jQuery(document).ready(function () {
	var authCookies = document.cookie.split(';').filter(function (cookie) {
		if (cookie.indexOf('is-authenticated=true') !== -1) {
			return cookie;
		}
	});

	if (authCookies.length > 0 && window.location.pathname === '/') {
		var menuWallet = jQuery('#menu-item-6242');
		var menuLogout = jQuery ('#menu-item-6243');
		var menuWalletMobile = jQuery('#menu-item-6242');
		var menuLogoutMobile = jQuery ('#menu-item-6243');

		menuWallet.find('a').attr('href', 'https://app.coins.ph/wallet').text('Go To Wallet');
		menuLogout.find('a').attr('href', 'https://app.coins.ph/welcome/logout').text('Log Out');
		menuWalletMobile.find('a').attr('href', 'https://app.coins.ph/wallet').text('Go To Wallet');
		menuLogoutMobile.find('a').attr('href', 'https://app.coins.ph/welcome/logout').text('Log Out');

		var ctaButtons = jQuery('a.mk-button');
		for (var i = 0; i < ctaButtons.length; i++) {
			ctaButton = ctaButtons[i];
			if (ctaButton.href === 'https://app.coins.ph/welcome/signup') {
				jQuery(ctaButton).attr('href', 'https://app.coins.ph/wallet').text('GO TO WALLET');
			}
		}
	}
