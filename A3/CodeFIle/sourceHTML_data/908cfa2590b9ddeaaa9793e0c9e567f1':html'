<script>
$(document).ready(function(){

	var imgs = $("#textinput").find('img');
	var imgC = 1;
	imgs.each(function (i) {
		var imgWidth = $(this).width() - 50;
		var imgSrc = $(this).attr('src');
		
		$(this).before('<div><div style="position: absolute; z-index: 999999999;"><div id="imgview_div'+imgC+'" style="position: relative; top: 10px; left: '+ imgWidth +'px;"><a href="/imgview.html?url='+ imgSrc +'" target="_blank"><img src="/img/icon_view.png?w=170628"></img></a></div></div>');
		$(this).after('</div>');
		imgC++;
	});

			var uid = 3398;
			var out = $('#mov');
			
            var data="uid="+uid;
	       
			if(movie)
	       {
			 $.ajax({
                data: data,
                type: 'POST',
                url: '/php_bin/movie_exe.php',
                
				success:function(args){
					var args = $.trim(args);
					//console.log(uid+'\n'+'\n'+args);
					if(args=="Error") {
						alert(args);
					} else {
						out.html(args);
					}
				},
				//beforeSend:showRequest,  
				error:function(request,status,error){  
					alert(request.status+":"+request.responseText+"::"+error);  
				}  
						 
            });

		   }
		   
	

   });
   
   $(window).bind("resize", function() {
		var imgs = $("#textinput").find('img');
		var imgC = 1;
		imgs.each(function (i) {
			if($(this).attr('src').indexOf('icon_view.png') != -1) {
			} else {
			var imgWidth = $(this).css('width').replace(/[^0-9]/g,"") - 50;
			$('#imgview_div'+imgC).css('left',imgWidth+'px');
			imgC++;
			}
		});
   });

	 
$(".cash").click(function() {

			var uid = $(".cash").attr('uid');
			var sc = $(".cash").attr('sc');
			var sc2 = $(".cash").attr('sc2');
			var mode = $(".cash").attr('mode');
			
			if(typeof sc=='undefined' || sc=='null' || sc=='undefined' || sc=='NaN'){
				sc = '';
			}
			
			if(typeof sc2=='undefined' || sc2=='null' || sc2=='undefined' || sc2=='NaN'){
				sc2 = '';
			}
			
			if(typeof mode=='undefined' || mode=='null' || mode=='undefined' || mode=='NaN'){
				mode = '';
			}			
			
            var data="d_mode=del";
				data +="&uid="+uid;
				data +="&sc="+sc;
				data +="&sc2="+sc2;
				data +="&mode="+mode;
	
			 $.ajax({
                data: data,
                type: 'POST',
                url: '/php_bin/cash_del_exe.php',
                
				success:function(args){
					var args = $.trim(args);
					//console.log(uid+'\n'+'\n'+args);
					if(args=="Error") {
						alert(args);
					} else {
						$(location).attr('href',args);
					}
				},
				//beforeSend:showRequest,  
				error:function(request,status,error){  
					alert(request.status+":"+request.responseText+"::"+error);  
				}  
			
            });

		});


</script>
