	$(document).on("click",".repost-submbtn",function(){
		var repost = $(this).attr("data-id");
		var postxt = $("#repost_txtbox").val();
		$.ajax({
			url: baseUrl+"/retweet-post",
			type: "POST",
			data: {repost:repost,postxt:postxt},
			success: function(data){
					$(".repost-tag[data-id="+repost+"]").find("span").text("Reposted");
					$("#repostModal").modal("hide");
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>Post has been reposted by you.</div></div>",
					});
					setTimeout(function(){
						location.reload();
					},700);
			}
		})
	})
	
	$(document).on("click",".community-btn",function(){		
		var subscribe = $(this).attr("data-id");
		var $this = $(this);
		$.ajax({
			url: baseUrl+"/subscribe-community",
			type: "POST",
			data: {subscribe:subscribe},
			success: function(data){
				var items = '';
		  		var items = $.parseJSON(data);
				if(items.state== 'updated') {
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>You have already subscribed the community.</div></div>",
					});
					setTimeout(function(){
						  $.unblockUI();
					},2000);
				} else if(items.state == 'failed') {
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>Please Log In to subscribe.</div></div>",
					});
					setTimeout(function(){
						  $.unblockUI();
					},2000);
				} else
				{
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>You have successfully subscribed the community.</div></div>",
					});
					setTimeout(function(){
						  $.unblockUI();
					},2000);
					$this.removeClass("community-btn");
					$this.addClass("community-txtbtn");
					$this.text("SUBSCRIBED");
					if(data.total > '1')
					{
						$(".community-paratxt[data-id="+subscribe+"]").html(items.total+' subscribers');
					} else {
						$(".community-paratxt[data-id="+subscribe+"]").html(items.total+' subscriber');
					}
				}
			}
		})
	})
	
