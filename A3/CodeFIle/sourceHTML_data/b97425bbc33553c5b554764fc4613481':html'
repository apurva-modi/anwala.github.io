.notification_li{position:relative;margin: 0 16px;}
.notification_bell_icon{position: absolute;top: 25px;left: 6px;cursor:pointer;}
header .notify_number {
    border-radius: 50%;
    background-color: #5a7dce;
    color: #fff;
    font-family: 'robotobold';
    position: absolute;
    width: 20px;
    text-align: center;
    top: -10px;
    right: -5px;
    font-size: 11px;
    height: 20px;
    line-height: 20px;
    z-index: 999;
