		$("#replytrashModal").modal("hide");
	})
	
	$(document).on("click",".trash-replybtn",function(){
		var trashid = $(this).attr("data-id");
		$("#replytrashModal").modal("show");
		$(".replytrash-btn").attr("data-id",trashid);
	})
	
	$(document).on("click",".replytrash-btn",function(){
		var trashid = $(this).attr("data-id");
		$.ajax({
			url:baseUrl+"/trash-reply",
			type:'post',
			data:{trashid:trashid},
			success:function(data){
				if(data=='success')
				{
					$("#coment"+trashid).remove();
					$("#replytrashModal").modal("hide");
				}
			}
		})
	})
	
	$(document).on("click",".commentreply-btn",function(){
		$(this).prop("disabled",true);
		var $this = $(this);
		$(".commentreply-errmsg").remove();
		var comment = CKEDITOR.instances.commentreply_message.getData();
		$(".commentreply-msgbox").val(comment);
		if($(".commentreply-msgbox").val() != '')
		{
			$.ajax({
					url:baseUrl+"/reply-comment",
					type:'post',
					data:$("#commentreply_form").serializeArray(),
					success:function(data){
						
						if(data == 'success')
						{
							$("#commentreplyModal").modal("hide");
							setTimeout(function(){
								location.reload();
							},100);	
							$this.prop("disabled",false);	
						}
					}
			})
		} else {
			$('<p class="commentreply-errmsg">This field is required</p>').insertAfter(".commentreply-msgbox");
			$this.prop("disabled",false);
		}
	})

	$(document).on("click",".fbshare-tag",function(){
		var title = $(this).attr("data-title");
		var descrip = $(this).attr("data-description");
		var pic = $(this).attr("data-pic");
		var facebookshare =  window.open("https://www.facebook.com/sharer/sharer.php?u=https://www.uptrennd.com/post-detail/"+title+"&description="+descrip+"&picture="+pic, "", "width=500,height=400");
	})
	
	$(document).on("click",".tweetshare-tag",function(){
		var title = $(this).attr("data-title");
		var descrip = $(this).attr("data-description");
		var pic = $(this).attr("data-pic");
		var twittershare =  window.open("https://twitter.com/intent/tweet?url=https://www.uptrennd.com/post-detail/"+title+"&description="+descrip+"&picture="+pic,"sharer","width=580,height=325");
	})
	
	$(document).on("click",".comment-reporttag",function(){
		var comment = $(this).attr("data-id");
		var comments = $(this).attr("data-vid");
		var type = $(this).attr("data-type");
		$("#flagcommentModal").modal("show");
		$(".flagcomment-btn").attr("data-id",comment);
		$(".flagcomment-btn").attr("data-vid",comments);
		$(".flagcomment-btn").attr("data-type",type);
	})
	
	$(document).on("click",".flagcomment-btn",function(){
		$(".flagcmnt-errmsg").remove();
		var comment = $(this).attr("data-id");
		var comments = $(this).attr("data-vid");
		var type = $(this).attr("data-type");
		if($("#flagcomment_message").val() != '')
		{
			var message = $("#flagcomment_message").val();
			$.ajax({
				url: baseUrl+"/report-comment",
				type: "POST",
				data: {comment:comment,comments:comments,message:message,type:type},
				success: function(data){
					$("#flagcommentModal").modal("hide");
					$("#flagcomment_message").val('');
					if(data == 'success'){
						$(".comment-reporttag[data-id="+comment+"]").remove();
						$.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#0b6fb2',
								opacity: .8,
								color: '#fff',
							},
							message: "<div class='Loader'><div class='Text'>Comment has been successfully reported.</div></div>",
						});
						setTimeout(function(){
							location.reload();
						},2000);			
					} else if(data == 'updates') {
						$.blockUI({
							css: {
								border:'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#0b6fb2',
								opacity: .8,
								color:'#fff',
							},
							message: "<div class='Loader'><div class='Text'>Comment has already been reported.</div></div>",
						});
						setTimeout(function(){
							location.reload();
						},2000);
					} else if(data == 'error') {
						$.blockUI({
							css: {
								border:'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#0b6fb2',
								opacity: .8,
								color:'#fff',
							},
							message: "<div class='Loader'><div class='Text'>Something went wrong. Please check again.</div></div>",
						});
						setTimeout(function(){
							location.reload();
						},2000);
					}
				}
			}) 
		} else {
			$('<p class="flagcmnt-errmsg">This field is required</p>').insertAfter("#flagcomment_message");
		}
	})
	
	$(document).on("click",".commentdown-anctag",function(){
