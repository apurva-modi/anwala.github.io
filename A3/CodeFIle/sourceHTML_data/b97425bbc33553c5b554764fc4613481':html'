});

$(document).on("click",".mute-cltbtn",function(){
	var muted = $(this).attr("data-id");
	$(".muteconfirms-btn").attr("data-id",muted);
	$("#mutesModal").modal("show");
})	

$(document).on("click",".muteconfirms-btn",function(){
	var mutes = $(this).attr("data-id");
	var mutetime = $(".mute-timestag").val();
	$(".mute-errmsg").remove();
	if(mutetime == ''){
		$('<p class="mute-errmsg">This field is required.</p>').insertAfter(".mute-timestag");
	} else {
		$.ajax({
			  url: baseUrl+"/mute-user",
			  type: "POST",
			  data: {keyword:mutes,mute:mutetime},
			  success: function(data){
				  $("#mutesModal").modal("hide");
				  $(".mute-cltbtn[data-id="+mutes+"]").remove();
			  }
		 })	
	}
})

$(document).ready(function(e) {	
		var srt = window.location.href;
	var segments = srt.split( '#' );
	if(segments[1] != '' && segments[1] != undefined){
		var comment = $("#comment-sorting").val();
		$.ajax({
			url: baseUrl+"/getall-comment",
			type: "POST",
			data: {post:'NTAwMw',comment:comment},
			success: function(data){
				$(".comment-outblock").html(data);
				$('html, body').animate({
					scrollTop: $('#'+segments[1]).offset().top
				}, 'slow');
			}
		})	
	}
	
	});

$(document).on("click",".postpic",function(){
	$(".img-zoom").attr("src",$(this).attr("src"));
	$("#imgzoomModal").modal("show");
})


