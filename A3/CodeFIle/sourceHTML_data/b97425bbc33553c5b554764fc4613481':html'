	$(document).on("click",".comment-newsort",function(){
		$('.comment-conversesort').removeClass('comment-activesort');
		$(".comment-topsort").removeClass("comment-activesort");
		$("#comment-sorting").val("2");
		$(this).addClass('comment-activesort');
		$.ajax({
			url:baseUrl+"/new-comments",
			type:'post',
			data:{postid:'NTAwMw'},
			success:function(data){
				if(data == 'failure'){
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>You cannot access this page.</div></div>",
					});
					setTimeout(function(){
						$.unblockUI();
						location.reload();
					},2000);
				} else {
					$(".comment-outblock").html(data);
					$(".replymore-txt").remove();
				}
			}
		})
	})
	
	$(document).on("click",".comment-conversesort",function(){
		$('.comment-newsort').removeClass('comment-activesort');
		$(".comment-topsort").removeClass('comment-activesort');
		$("#comment-sorting").val("3");
		$(this).addClass('comment-activesort');
		
		$.ajax({
			url:baseUrl+"/converse-comments",
			type:'post',
			data:{postid:'NTAwMw'},
