	$(document).on("click",".vote-arrow",function(){
		var post = $(this).attr("data-id");
		var vote = $(this).attr("data-vote");
		var $this = $(this);
		$.ajax({ 
			  url: baseUrl+"/save-vote",
			  type: "POST",
			  data: {post:post,vote:vote},
			  success: function(data){	
				  if(data == 'failed') {
					  $.blockUI({
						  css: {
							  border: 'none',
							  padding: '5px',
							  width: '24%',
							  left: '39%',
							  backgroundColor: '#0b6fb2',
							  opacity: .8,
							  color: '#fff',
						  },
						  message: "<div class='Loader'><div class='Text'>Please login to your account.</div></div>",
					  });
					  setTimeout(function(){
						  $.unblockUI();
					  },2000);
				  }else if(data == 'finish'){
						
						$("#votesModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can only vote a unique user three times in one day.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					}else if(data == 'errors') {
					  $.blockUI({
						  css: {
							  border: 'none',
							  padding: '5px',
							  width: '24%',
							  left: '39%',
							  backgroundColor: '#0b6fb2',
							  opacity: .8,
							  color: '#fff',
						  },
						  message: "You can't upvote your own post.",
					  });
					  setTimeout(function(){
						  $.unblockUI();
					  },2000);
					  
				  }else  if(data == 'already') {
					  $.blockUI({
						  css: {
							  border: 'none',
							  padding: '5px',
							  width: '24%',
							  left: '39%',
							  backgroundColor: '#0b6fb2',
							  opacity: .8,
							  color: '#fff',
						  },
						  message: "You have already voted this post.",
					  });
					  setTimeout(function(){
						  $.unblockUI();
					  },2000);
					  
				  } else {			
				  	$(".news-acttxt[data-id="+post+"]").text(data);
					if(vote == '4'){
						$this.attr("src","https://www.uptrennd.com/images/upact-arrow.svg");
						$(".down-arrow[data-id="+post+"]").attr("src","https://www.uptrennd.com/images/down-arrow.svg");
					} else {
						$this.attr("src","https://www.uptrennd.com/images/downact-arrow.svg");
						$(".up-arrow[data-id="+post+"]").attr("src","https://www.uptrennd.com/images/up-arrow.svg");
					}
				  }
			  }
		 });
	})
	
	$(document).on("click",".subscribe-btn",function(){
		var subscribe = $(this).attr("data-id");
		var $this = $(this);
		$.ajax({
			url: baseUrl+"/subscribe-community",
			type: "POST",
			data: {subscribe:subscribe},
			success: function(data){
				var items = '';
		  		var items = JSON.parse(data);
				if(items.success== 'updated') {
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>You have already subscribed the community.</div></div>",
					});
					setTimeout(function(){
						  $.unblockUI();
					},2000);
				} else if(items.success == 'failed') {
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>Please Log In to subscribe.</div></div>",
					});
					setTimeout(function(){
						  $.unblockUI();
					},2000);
				} else
				{
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>You have successfully subscribed the community.</div></div>",
					});
					setTimeout(function(){
						  $.unblockUI();
					},2000);
					$this.removeClass("subscribe-btn");
					$this.addClass("subscribed-btn");
					$this.text("SUBSCRIBED");
					$(".subscriber-tottag").html(items.total);
					if(data > '1')
					{
						$(".subscriber-tottag").next(".subscriber-paratag").text("Subscribers");
					} else {
						$(".subscriber-tottag").next(".subscriber-paratag").text("Subscriber");
					}
				}
			}
		})
	})
	
	$(document).ready(function(e) {
       	    });
	
	$(document).ready(function(e) {
		if (CKEDITOR.instances['post_text']) {
			//CKEDITOR.remove(CKEDITOR.instances['post_text']);
		} else{
			CKEDITOR.replace('post_text',{toolbar :[['Source'],['Bold','Italic','Underline','Undo','Redo'],['JustifyLeft', 'JustifyCenter', 'JustifyRight', 'JustifyBlock'],['TextColor'],
		['Format'],['FontSize'],['Image'],['Youtube'],['Smiley']],height: 250,language:"en",});
			CKEDITOR.config.filebrowserUploadUrl = baseUrl+'/upload-mediapics';
			CKEDITOR.config.filebrowserImageBrowseUrl = baseUrl+'/browse-mediapics';
			
			CKEDITOR.instances['post_text'].on("key", function(event){
				if (event.data.keyCode == CKEDITOR.CTRL + 90){
					event.cancel();	
				}
				else if (event.data.keyCode == CKEDITOR.CTRL + 89){
					event.cancel();
				}
			})
		}
		
		if (CKEDITOR.instances['commentreply_message']) {
			//CKEDITOR.remove(CKEDITOR.instances['post_text']);
		} else{
			CKEDITOR.replace('commentreply_message',{startupFocus : true,toolbar :[['Source'],['Bold','Italic','Underline','Undo','Redo'],['JustifyLeft', 'JustifyCenter', 'JustifyRight', 'JustifyBlock'],['TextColor'],
		['Format'],['FontSize'],['Image'],['Youtube'],['Smiley']],height: 250,language:"en",});
			CKEDITOR.config.filebrowserUploadUrl = baseUrl+'/upload-mediapics';
			CKEDITOR.config.filebrowserImageBrowseUrl = baseUrl+'/browse-mediapics';
						
			CKEDITOR.instances['commentreply_message'].on("key", function(event){
				if (event.data.keyCode == CKEDITOR.CTRL + 90){
					event.cancel();	
				}
				else if (event.data.keyCode == CKEDITOR.CTRL + 89){
					event.cancel();
				}
			})
		}
		
		if (CKEDITOR.instances['postupd_message']) {
			//CKEDITOR.remove(CKEDITOR.instances['post_text']);
		} else{
			CKEDITOR.replace('postupd_message',{startupFocus : true,toolbar :[['Source'],['Bold','Italic','Underline'],['JustifyLeft', 'JustifyCenter', 'JustifyRight', 'JustifyBlock'],['TextColor'],
		['Format'],['FontSize'],['Image'],['Youtube']],height: 250,language:"en",});
			CKEDITOR.config.filebrowserUploadUrl = baseUrl+'/upload-mediapics';
			CKEDITOR.config.filebrowserImageBrowseUrl = baseUrl+'/browse-mediapics';
		}
		
		if (CKEDITOR.instances['postupd_reply']) {
			//CKEDITOR.remove(CKEDITOR.instances['post_text']);
		} else{
			CKEDITOR.replace('postupd_reply',{startupFocus : true,toolbar :[['Source'],['Bold','Italic','Underline'],['JustifyLeft', 'JustifyCenter', 'JustifyRight', 'JustifyBlock'],['TextColor'],
		['Format'],['FontSize'],['Image'],['Youtube']],height: 250,language:"en",});
			CKEDITOR.config.filebrowserUploadUrl = baseUrl+'/upload-mediapics';
			CKEDITOR.config.filebrowserImageBrowseUrl = baseUrl+'/browse-mediapics';
		}
		
		jQuery('.cke_wysiwyg_frame').contents().find('html').css("min-height","250px");
	})	
	
	$(document).ready(function ()
{
    if (/iPhone/.test(navigator.userAgent) && !window.MSStream)
    {
        $(document).on("focus", "input, textarea, select", function()
        {
            $('meta[name=viewport]').remove();
            $('head').append('<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=0">');
        });

        $(document).on("blur", "input, textarea, select", function()
        {
            $('meta[name=viewport]').remove();
            $('head').append('<meta name="viewport" content="width=device-width, initial-scale=1">');
        });
    }
});


$(document).on("click",".postmsg-updbtn",function(){
	var trp = $(this).attr("data-vid");
	var msg = $(this).parents(".comment-header").next(".comment-para").html();
	if (CKEDITOR.instances['postupd_message']) {
		var editor1 = CKEDITOR.instances['postupd_message'];
		editor1.setData(msg);
	}
	$(".msgupd-btn").attr("data-id",trp);
	$(".msgupd-btn").attr("data-pid","NTAwMw");
	$("#msgupdModal").modal("show");
})	

$(document).on("click",".postreply-updbtn",function(){
	var trp = $(this).attr("data-vid");
	var msg = $(this).parents(".comment-header").next(".comment-para").html();
	if (CKEDITOR.instances['postupd_reply']) {
		var editor1 = CKEDITOR.instances['postupd_reply'];
		editor1.setData(msg);
	}
	$(".replyupd-btn").attr("data-id",trp);
	$(".replyupd-btn").attr("data-pid","NTAwMw");
	$("#replyupdModal").modal("show");
	
})

$(document).on("click",".msgupd-btn",function(){
	var trv = $(this).attr("data-id");
	var pid = $(this).attr("data-pid");
	if (CKEDITOR.instances['postupd_message']) {
		var editor1 = CKEDITOR.instances['postupd_message'];
		var msg = editor1.getData();
	}
	$.ajax({
		url: baseUrl+"/update-postmsg",
		type: "POST",
		data: {trv:trv,pid:pid,msg:msg},
		success: function(data){
			if(data == 'success'){
				$(".comment-sector[data-id="+trv+"]").children(".comment-header").next(".comment-para").html(msg);
				$("#msgupdModal").modal("hide");
			}
		}
	})
})

$(document).on("click",".replyupd-btn",function(){
	var trv = $(this).attr("data-id");
	var pid = $(this).attr("data-pid");
	if (CKEDITOR.instances['postupd_reply']) {
		var editor1 = CKEDITOR.instances['postupd_reply'];
		var msg = editor1.getData();
	}
	$.ajax({
		url: baseUrl+"/update-postreply",
		type: "POST",
		data: {trv:trv,pid:pid,msg:msg},
		success: function(data){
			if(data == 'success'){
				$(".comment-sector[data-reply="+trv+"]").children(".comment-header").next(".comment-para").html(msg);
				$("#replyupdModal").modal("hide");
			}
		}
	})
})

function getDocumentHeight() {
return Math.max(
    Math.max(document.body.scrollHeight, document.documentElement.scrollHeight),
    Math.max(document.body.offsetHeight, document.documentElement.offsetHeight),
    Math.max(document.body.clientHeight, document.documentElement.clientHeight)
);
}

