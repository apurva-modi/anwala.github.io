	$(document).on("click",".reply-tag",function(){
		var $this = $(this);
		$.ajax({
				url: baseUrl+"/getmute-stat",
				type: "POST",
				data: {mute:'1'},
				success: function(data){
					if(data == '1'){
						$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
							message: "<div class='Loader'><div class='Text'>You cannot comment as you have been muted by the moderator.</div></div>",
						});
						setTimeout(function(){
							  $.unblockUI();
						},2000);
					} else {
		
		var comment = $this.attr("data-id");
		var comments = $this.attr("data-vid");
		var pid = $this.attr("data-pid");
		var pmid = $this.attr("data-mid"); 
		var pos = $this.attr("data-pos");
				
		$("#commentreplyModal").modal("show");
		$(".commentreply-msgbox").focus();
		$("#commentreply_msg").val(comment);
		$("#comment_reply").val(comments);
		$("#commentreply_pid").val(pid);
		$("#commentreply_mid").val(pmid);
		$("#commentreply_pos").val(pos);
		$("#commentreply_post").val('NTAwMw');
					}
				}
		})
	})
	
	$("#commentreplyModal").on('shown.bs.modal', function(){
		if (CKEDITOR.instances['commentreply_message']) { CKEDITOR.instances['commentreply_message'].destroy(true); }
			CKEDITOR.replace('commentreply_message',{startupFocus : true,toolbar :[['Source'],['Bold','Italic','Underline','Undo','Redo'],['JustifyLeft', 'JustifyCenter', 'JustifyRight', 'JustifyBlock'],['TextColor'],
		['Format'],['FontSize'],['Image'],['Youtube'],['Smiley']],height: 250,language:"en",});
			CKEDITOR.config.filebrowserUploadUrl = baseUrl+'/upload-mediapics';
			CKEDITOR.config.filebrowserImageBrowseUrl = baseUrl+'/browse-mediapics';
			
			CKEDITOR.instances['commentreply_message'].on("key", function(event){
				if (event.data.keyCode == CKEDITOR.CTRL + 90){
					event.cancel();	
				}
				else if (event.data.keyCode == CKEDITOR.CTRL + 89){
					event.cancel();
				}
			})
		})
		

	jQuery(function ($) {
    $('.trash-replybtn').hover(function () {
        $(this).find('img').attr('src', function (i, src) {
            return src.replace('trash.svg', 'trash_hover.svg')
        })
    }, function () {
        $(this).find('img').attr('src', function (i, src) {
            return src.replace('trash_hover.svg', 'trash.svg')
        })
    	})
	})
	
	jQuery(function ($) {
    $('.postmsg-delbtn').hover(function () {
        $(this).find('img').attr('src', function (i, src) {
            return src.replace('trash.svg', 'trash_hover.svg')
        })
    }, function () {
        $(this).find('img').attr('src', function (i, src) {
            return src.replace('trash_hover.svg', 'trash.svg')
        })
    	})
	})
	
	
	$(document).on("click",".postmsg-delbtn",function(){
		var trashid = $(this).attr("data-vid");
		var trashid = trashid.replace("=","");
		$("#postcmttrashModal").modal("show");
		$(".postcmttrash-btn").attr("data-id",trashid);
	})
	
	$(document).on("click",".comment-unsavetag",function(){
		var cmnt = $(this).attr("data-id");
		var postid = $(this).attr("data-pid");
		var type = $(this).attr("data-type");
		$.ajax({
			url:baseUrl+"/unsave-comment",
			type:'post',
			data:{cmnt:cmnt,postid:postid,type:type},
			success:function(data){
				if(data == 'success')
				{
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>You have unsaved the comment successfully.</div></div>",
					});
					setTimeout(function(){
						$.unblockUI();
						location.reload();
					},2000);	
				
			} else if(data == 'error') {
				$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>Something went wrong. Please check again.</div></div>",
					});
					setTimeout(function(){
						$.unblockUI();
						location.reload();
					},2000);
			}
			}
		})
	})
	
	$(document).on("click",".comment-savetag",function(){
		var cmnt = $(this).attr("data-id");
		var postid = $(this).attr("data-pid");
		var type = $(this).attr("data-type");
		$.ajax({
			url:baseUrl+"/save-comment",
			type:'post',
			data:{cmnt:cmnt,postid:postid,type:type},
			success:function(data){
				if(data == 'success')
				{
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
						message: "<div class='Loader'><div class='Text'>You have saved the comment successfully.</div></div>",
					});
					setTimeout(function(){
						$.unblockUI();
						location.reload();
					},2000);	
				} else if(data == 'updated')
				{
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
					},
						message: "<div class='Loader'><div class='Text'>You have already saved the comment successfully.</div></div>",
					});
					setTimeout(function(){
						$.unblockUI();
						location.reload();
					},2000);
				} else if(data == 'error') {
					$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
					},
						message: "<div class='Loader'><div class='Text'>Something went wrong. Please check again.</div></div>",
					});
					setTimeout(function(){
						$.unblockUI();
						location.reload();
					},2000);
				}
			}
		})
	})
	
	$(document).on("click",".postcmttrash-btn",function(){
		var postcmt = $(this).attr("data-id");
		$.ajax({
			url:baseUrl+"/trash-postcmnt",
			type:'post',
			data:{postcmt:postcmt},
			success:function(data){
				if(data == 'success')
				{
					$("#postcmttrashModal").modal("hide");
					$("#post"+postcmt).remove();
					$(".comment-sector[data-id="+postcmt+"]").remove();
					
				}
			}
		})
	})
	
