	$(document).on("click",".donate-pointtag",function(){
		var donate = $(this).attr("data-id");
		var donates = $(this).attr("data-pid");
		var donet = $(this).attr("data-tid");
		$("#donatepointModal").modal("show");
		$(".donatepoint-submbtn").attr("data-id",donate);
		$(".donatepoint-submbtn").attr("data-pid",donates);
		$(".donatepoint-submbtn").attr("data-tid",donet);
	})
	
	$(document).on("click",".donatepoint-submbtn",function(){
		var donate = $(this).attr("data-id");
		var donates = $(this).attr("data-pid");
		var donet = $(this).attr("data-tid");
		$(".donate-errmsg").remove();
		var donate_txt = $("#donate_pointbox").val();
		if(donate_txt == '')
		{
			$('<p class="donate-errmsg">This field is required</p>').insertAfter("#donate_pointbox");
		} else {
			$.ajax({
				url: baseUrl+"/donate-commentpoint",
				type: "POST",
				data: {donate:donate,donate_txt:donate_txt,donates:donates,donet:donet},
				success: function(data){
					$("#donatepointModal").modal("hide");
					if(data == 'insufficient')
					{
						$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
							message: "<div class='Loader'><div class='Text'>You do not have enough points to donate.</div></div>",
						});
						setTimeout(function(){
							  $.unblockUI();
						},2000);
					} else if(data == 'error')
					{
						$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
							message: "<div class='Loader'><div class='Text'>Please login to your account.</div></div>",
						});
						setTimeout(function(){
							  $.unblockUI();
						},2000);
					} else if(data == 'failure')
					{
						$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
							message: "<div class='Loader'><div class='Text'>Something went wrong. Please check again.</div></div>",
						});
						setTimeout(function(){
							  $.unblockUI();
						},2000);
					} else if(data == 'success')
					{
						$.blockUI({
						css: {
							border: 'none',
							padding: '5px',
							width: '24%',
							left: '39%',
							backgroundColor: '#0b6fb2',
							opacity: .8,
							color: '#fff',
						},
							message: "<div class='Loader'><div class='Text'>You have donated successfully.</div></div>",
						});
						setTimeout(function(){
							  $.unblockUI();
							  location.reload();
						},2000);
					}
				}
			})
		}
	})
	
	$(document).on("click",".donate-tag",function(){
		var donate = $(this).attr("data-id");
		$("#donateModal").modal("show");
		$(".donate-submbtn").attr("data-id",donate);
	})
	
