<script type="text/javascript">
jQuery(function($) {
    $('.notify_collapse').on('scroll', function() {
          var length=  $("#notify_content .total-mediatg ").length;
	   	  var start = length;
		  var total=0;
		if($(this).scrollTop() + $(this).innerHeight() >= $(this)[0].scrollHeight) {
			if(total > start){
				$.ajax({
					url: baseUrl+"/getnotificationsection",
					type: "POST",
					data: {start:start},
					success: function(data){
						$("#notify_content").append(data);
						$(".loader-pic").addClass("hide");
					}
				})
			}
        }
    })
});

	$(document).on("click",".open-close",function(){
		var tog_stat = $(this).attr("data-id");
		if(tog_stat == '1')
		{
			$(".dash-logo").attr("src","https://www.uptrennd.com/images/back_logo.png");
			$(this).attr("data-id","0");
		} else {
			$(".dash-logo").attr("src","https://www.uptrennd.com/avatars/logo/logo-1541138738.png");
			$(this).attr("data-id","1");
		}
	})
	
	$(document).on("click",".notification_read",function(){
		var unread_count = $("#unreadNotificationCount").text();
		$.ajax({
			url: baseUrl+"/notify-chk",
			type: "POST",
			data: {notify:'1'},
			success: function(data){
				$(".notify_number").text('0');
				if(unread_count > 0){
					$("#openNC").trigger("click");
					$("#unreadNotificationCount").text('0');
				}
			}
		})
	})
	
	$(document).on("click",".searchbtn-pic",function(){
		$(".profile_form12").submit();
	})
	
	$(document).on("click",".search-btnpic",function(){
		$(".searchbar-section").toggleClass("hide");
	})	
</script>
 
