<script type="text/javascript">
	var FRONT_CSSURL = 'https://www.uptrennd.com/assets/css';
	$(document).ready(function(e) {
        $(".login-btn").attr("href","https://www.uptrennd.com/login?url=%2Fpost-detail/happy-tax-you-may-not-think-anything-about-taxes-are-happy-but-at-least-these-guys-are-happy-to-help-with-your-crypto-tax-solutions~NTAwMw==");
	});
	
	$(document).on("click",".votes-arrow",function(){
		var upvote = $(this).attr("data-id");
		var vote = $(this).attr("data-vote");
		if(vote == '4') {
			$(".upvote-btntag").attr("data-id",upvote);
			$("#votesModal").modal("show");
		} else {
			$(".downvote-btntag").attr("data-id",upvote);
			$("#downvotesModal").modal("show");
		}
	})
	
	$(document).on("click",".commentmod-upltag",function(){
		var comment = $(this).attr("data-id");
		var post = $(this).attr("data-vid");
		$(".uplvote-modbtn").attr("data-id",comment);
		$(".uplvote-modbtn").attr("data-vid",post);
		$("#uplcommentModal").modal("show");
	})
	
	$(document).on("click",".commentmod-dwltag",function(){
		var comment = $(this).attr("data-id");
		var post = $(this).attr("data-vid");
		$(".dwlvote-modbtn").attr("data-id",comment);
		$(".dwlvote-modbtn").attr("data-vid",post);
		$("#dwlcommentModal").modal("show");
	})
		
	$(document).on("click",".commentup-modtag",function(){
		var comment = $(this).attr("data-id");
		var post = $(this).attr("data-vid");
		$(".upvote-modbtn").attr("data-id",comment);
		$(".upvote-modbtn").attr("data-vid",post);
		$("#upcommentModal").modal("show");
	})
	
	$(document).on("click",".commentdown-modtag",function(){
		var comment = $(this).attr("data-id");
		var post = $(this).attr("data-vid");
		$(".downvote-modbtn").attr("data-id",comment);
		$(".downvote-modbtn").attr("data-vid",post);
		$("#downcommentModal").modal("show");
	})
	
	$(document).on("click",".uplvote-modbtn",function(){
		var comment = $(this).attr("data-id");
		var vote = $("#uplvotes_txt").val();
		var post = $(this).attr("data-vid");
		var $this = $(this);
		$(this).prop("disabled",true);
		$.blockUI({
			css: {
				border: 'none',
				padding: '5px',
				width: '24%',
				left: '39%',
				backgroundColor: '#5a7dce',
				opacity: .8,
				color: '#fff',
			},
			message: "Please wait...",
		});
		$.ajax({
				url: baseUrl+"/uplvotes-post",
				type: "POST",
				data: {comment:comment,vote:vote,post:post},
				success: function(data){
					 $.unblockUI();
					if(data == 'errors')
					{
						$("#uplcommentModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can't upvote your own comment.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					} else if(data == 'finish'){
						
						$("#uplcommentModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can only vote a unique user three times in one day.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					} else {
						$("#uplcommentModal").modal("hide");
						$this.prop("disabled",false);
						$(".commentmod-upltag[data-id="+comment+"]").find("img").attr("src","https://www.uptrennd.com/images/upact-arrow.svg");
						$(".commentmod-dwltag[data-id="+comment+"]").find("img").attr("src","https://www.uptrennd.com/images/down-arrow.svg");
						$(".replies-acttxt[data-id="+atob(comment)+"]").text(data);
					}
				}
		})
	})
	
	$(document).on("click",".dwlvote-modbtn",function(){
		var comment = $(this).attr("data-id");
		var vote = $("#dwlvotes_txt").val();
		var post = $(this).attr("data-vid");
		var $this = $(this);
		$(this).prop("disabled",true);
		$.ajax({
				url: baseUrl+"/dwlvotes-post",
				type: "POST",
				data: {comment:comment,vote:vote,post:post},
				success: function(data){
					if(data == 'errors'){						
						$("#dwlcommentModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can't downvote your own post.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					} else {
					$("#dwlcommentModal").modal("hide");
					$this.prop("disabled",false);
					$(".replies-acttxt[data-id="+atob(comment)+"]").text(data);
					$(".commentmod-dwltag[data-id="+comment+"]").find("img").attr("src","https://www.uptrennd.com/images/downact-arrow.svg");
					$(".commentmod-upltag[data-id="+comment+"]").find("img").attr("src","https://www.uptrennd.com/images/up-arrow.svg");
					}
				}
		})
	})
	
	
	$(document).on("click",".downvote-modbtn",function(){
		var comment = $(this).attr("data-id");
		var vote = $("#dnvotes_txt").val();
		var post = $(this).attr("data-vid");
		var $this = $(this);
		$(this).prop("disabled",true);
		var cmt = atob(comment);
		$.ajax({
				url: baseUrl+"/downvotes-post",
				type: "POST",
				data: {comment:comment,vote:vote,post:post},
				success: function(data){
					if(data == 'errors'){						
						$("#downcommentModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can't downvote your own comment.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					} else {$("#upcommentModal").modal("hide");
					$this.prop("disabled",false);
					$(".cmnt-acttxt[data-id="+cmt+"]").html(data);
					$(".commentup-modtag[data-id="+comment+"]").find(".comment-uptag").attr("src","https://www.uptrennd.com/images/up-arrow.svg");
					$(".commentdown-modtag[data-id="+comment+"]").find(".comment-downtag").attr("src","https://www.uptrennd.com/images/downact-arrow.svg");
					$("#downcommentModal").modal("hide");
					}
				}
		})
	})
	
	$(document).on("click",".upvote-modbtn",function(){
		var comment = $(this).attr("data-id");
		var vote = $("#upvotes_txt").val();
		var post = $(this).attr("data-vid");
		var $this = $(this);
		$(this).prop("disabled",true);
		var cmt = atob(comment);
		$.blockUI({
			css: {
				border: 'none',
				padding: '5px',
				width: '24%',
				left: '39%',
				backgroundColor: '#5a7dce',
				opacity: .8,
				color: '#fff',
			},
			message: "Please wait...",
		});
		$.ajax({
				url: baseUrl+"/upvotes-post",
				type: "POST",
				data: {comment:comment,vote:vote,post:post},
				success: function(data){
					$.unblockUI();
					if(data == 'errors'){						
						$("#upcommentModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can't upvote your own comment.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					} else if(data == 'finish'){
						
						$("#upcommentModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can only vote a unique user three times in one day.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					}
					 else {
						$("#upcommentModal").modal("hide");
						$this.prop("disabled",false);
						$(".cmnt-acttxt[data-id="+cmt+"]").html(data);
						$(".commentdown-modtag[data-id="+comment+"]").find(".comment-downtag").attr("src","https://www.uptrennd.com/images/down-arrow.svg");
						$(".commentup-modtag[data-id="+comment+"]").find(".comment-uptag").attr("src","https://www.uptrennd.com/images/upact-arrow.svg")
					}
				}
		})
	})
	
	$(document).on("click",".upvote-btntag",function(){
		var upvote = $(this).attr("data-id");
		var vote = $("#vote_txt option:selected").val();
		var vte = $("input[name=vote_csrf]").val();
		var $this = $(this);
		$(this).prop("disabled",true);
		$.blockUI({
			css: {
				border: 'none',
				padding: '5px',
				width: '24%',
				left: '39%',
				backgroundColor: '#5a7dce',
				opacity: .8,
				color: '#fff',
			},
			message: "Please wait...",
		});
		$.ajax({
				url: baseUrl+"/upvote-post",
				type: "POST",
				data: {upvote:upvote,vote:vote,vte:vte},
				success: function(data){
					$.unblockUI();
					if(data != 'errors'  && data != 'finish'){	
						
						$this.prop("disabled",false);
						$(".news-acttxt[data-id="+upvote+"]").html(data);
						$("#votesModal").modal("hide");
						$(".up-arrow[data-id="+upvote+"]").attr("src","https://www.uptrennd.com/images/upact-arrow.svg");
						$(".down-arrow[data-id="+upvote+"]").attr("src","https://www.uptrennd.com/images/down-arrow.svg");
					}else if(data == 'finish'){
						
						$("#votesModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can only vote a unique user three times in one day.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					}else{
						$("#votesModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can't upvote your own post.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					}
				}
		})
	})
	
	$(document).on("click",".downvote-btntag",function(){
		var downvote = $(this).attr("data-id");
		var vote = $("#votes_txt option:selected").val();
		var vte = $("input[name=votes_csrf]").val();
		var $this = $(this);
		$(this).prop("disabled",true);
		$.ajax({
				url: baseUrl+"/downvote-post",
				type: "POST",
				data: {downvote:downvote,vote:vote,vte:vte},
				success: function(data){
					if(data == 'errors'){						
						$("#downvotesModal").modal("hide");
						$this.prop("disabled",false);
						 $.blockUI({
							css: {
								border: 'none',
								padding: '5px',
								width: '24%',
								left: '39%',
								backgroundColor: '#5a7dce',
								opacity: .8,
								color: '#fff',
							},
							message: "You can't downvote your own post.",
						});
						setTimeout(function(){  $.unblockUI() }, 2000);
					} else {
					$("#downvotesModal").modal("hide");
					$this.prop("disabled",false);
					$(".news-acttxt[data-id="+downvote+"]").html(data);
					$(".up-arrow[data-id="+downvote+"]").attr("src","https://www.uptrennd.com/images/up-arrow.svg");
					$(".down-arrow[data-id="+downvote+"]").attr("src","https://www.uptrennd.com/images/downact-arrow.svg");
					}
				}
		})
	})

