		$(".news-block").css("overflow","hidden");
		$(".social-sharelist").toggle();
	})
	
	$(document).on("click",".replymore-txt",function(){
		$.ajax({
			url: baseUrl+"/getall-comment",
			type: "POST",
			data: {post:'NTAwMw'},
			success: function(data){
				$(".comment-outblock").html(data);
				$(".replymore-txt").remove();
			}
		})
	})

	$(document).on("click",".comment-btn",function(){
		var $this = $(this);
		$(this).prop("disabled",true);
		$.ajax({
				url: baseUrl+"/getmute-stat",
				type: "POST",
				data: {mute:'1'},
				success: function(data){
					if(data == '1'){
			$.blockUI({
			css: {
				border: 'none',
				padding: '5px',
				width: '24%',
				left: '39%',
				backgroundColor: '#0b6fb2',
				opacity: .8,
				color: '#fff',
			},
				message: "<div class='Loader'><div class='Text'>You cannot comment as you have been muted by the moderator.</div></div>",
			});
			setTimeout(function(){
				  $.unblockUI();
			},2000);
				$this.prop("disabled",false);
				$(".comment-topsort").removeClass("comment-activesort");
				$(".comment-newsort").addClass("comment-activesort");
					} else {
		var comment = CKEDITOR.instances.post_text.getData();
		$(".comment-emptag").remove();
		$(this).prop("disabled",true);
		
		if(comment == '')
		{
			$('<p class="comment-emptag">This field is required</p>').insertAfter(".comment-btn");
			$this.prop("disabled",false);
		} else {
			$.blockUI({
				css: {
					border: 'none',
					padding: '5px',
					width: '24%',
					left: '39%',
					backgroundColor: '#0b6fb2',
					opacity: .8,
					color: '#fff',
			},
				message: "<div class='Loader'><div class='Text'>Please wait.</div></div>",
			});
			$.ajax({ 
				url: baseUrl+"/post-comment",
				type: "POST",
				data: {comment:comment,post:'5003'},
				success: function(data){
					$(".comment-outblock").html(data);
					CKEDITOR.instances.post_text.setData('');
					$this.prop("disabled",false);
					$.unblockUI();
				}
			})
		}
					}
				}
	})
	})
	
	
