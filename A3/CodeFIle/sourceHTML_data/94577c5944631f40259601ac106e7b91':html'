  function getDownloadURL(id) {
    var isMac = navigator.platform.toUpperCase().indexOf('MAC') >= 0;
    var result = {};

    if (isMac) {
      result.href = 'https://get.cryptobrowser.site/get/CryptoTabBrowser.dmg';
      result.download = 'CryptoTabBrowser' + (id ? '_' + id : '') + '.dmg';
      return result;
    }

    result.href = 'https://get.cryptobrowser.site/get/CryptoTabSetup.exe';
    result.download = 'CryptoTabSetup' + (id ? '_' + id : '') + '.exe';
    return result;
  }

  function initDownloadButtons (id) {
    var attributes = getDownloadURL(id)

    var downloadBtns = document.querySelectorAll(downloadSelector)
    for (var i = 0; i < downloadBtns.length; i++) {
      downloadBtns[i].setAttribute('href', attributes.href)
      downloadBtns[i].setAttribute('download', attributes.download)
    }

    
  }

  initDownloadButtons()

