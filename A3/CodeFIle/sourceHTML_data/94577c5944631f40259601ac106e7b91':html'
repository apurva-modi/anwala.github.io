<script src="/static/scripts/countUp.min.js"></script>
<script type="text/javascript">
    // Header notification
    var hideHeaderNotification;
    var headerNotification = document.getElementById('headerNotification');
    var page = document.querySelector('.page');
    var hidePromoSharingPopup;
    var promoSharingPopup = document.getElementById('promoSharingPopup');
    var allow = false;
    var easyMode = false;
    var bonusMultiplier = 1;
    var bonusAmount = 0.00001;
    var bonusAmountSatoshi = 1000;
    var userTz = new Date().getTimezoneOffset()
    
    try {
        hideHeaderNotification = sessionStorage.getItem('hideHeaderNotification');
        hidePromoSharingPopup = localStorage.getItem('hidePromoSharingPopup');
    } catch (e) {
        
    }

    function checkPromoSharing(trackId, isLogin) {
        var xhr = new XMLHttpRequest();
        xhr.open('POST', 'https://cryptobrowser.site/promo/sharing/check/', true);
        xhr.setRequestHeader('Content-Type', 'application/json');
        xhr.withCredentials = true;
        xhr.send(JSON.stringify({tz:userTz, track_id: trackId, login: isLogin}));
        xhr.onreadystatechange = function() {
            try {
                if (xhr.readyState != 4) return;

                if (xhr.status == 200) {
                    var data = JSON.parse(xhr.responseText);
                    allow = data.allow;

                    if (data.em) {
                        easyMode = data.em;
                    }

                    if (data.amount && data.amount != 0.00001) {
                        bonusAmount = (data.amount).toFixed(8);
                        bonusMultiplier = Math.round(data.amount/0.00001*100)/100;
                        bonusAmountSatoshi = Math.round(data.amount/0.00000001);
                    }

                    if (easyMode) {
                        if (bonusMultiplier > 1) {
                            //promoSharingPopup.querySelector('.popup__header .btc-bonus i').innerHTML = bonusAmount;
                            promoSharingPopup.querySelector('.popup__body').innerHTML = createStringFromTemplate('<h2>Your personal bonus is 0.00001 x {js_bonus_multiplier} = {js_bonus_amount} BTC.</h2><p><b>You’re the lucky one – <span class="orange">your bonus multiplier is x{js_bonus_multiplier}</span></b>. Regular bonus for repost is 0.00001000 BTC (1000 Satoshi), your personal bonus – {js_bonus_amount} BTC (<b>{js_bonus_satoshi} Satoshi</b>)! Support the mining community by making a repost and get a bonus on your account balance.</p>', {js_bonus_amount: bonusAmount, js_bonus_multiplier: bonusMultiplier, js_bonus_satoshi: bonusAmountSatoshi});
                            headerNotification.querySelector('.notification__title').innerHTML = createStringFromTemplate('Grab your personal repost bonus of <b>{js_bonus_amount} BTC for free</b>', {js_bonus_amount: bonusAmount});
                        } else {
                            //promoSharingPopup.querySelector('.popup__header .btc-bonus i').innerHTML = bonusAmount.toFixed(8);
                            promoSharingPopup.querySelector('.popup__body').innerHTML = '<h2>Support the mining community by making a repost and get a bonus!</h2><p>Support the mining community by making a repost and get a bonus of 0.00001000 BTC (1000 Satoshi) on your account.</p>';
                            headerNotification.querySelector('.notification__title').innerHTML = 'Grab your personal bonus of up to <b>0.00001000 BTC for free</b>';
                        }
                    } else {
                        if (bonusMultiplier > 1) {
                            //promoSharingPopup.querySelector('.popup__header .btc-bonus i').innerHTML = bonusAmount;
                            promoSharingPopup.querySelector('.popup__body').innerHTML = createStringFromTemplate('<h2>Your personal bonus is 0.00001 x {js_bonus_multiplier} = {js_bonus_amount} BTC.</h2><p><b>You’re the lucky one – <span class="orange">your bonus multiplier is x{js_bonus_multiplier}</span></b>. Regular bonus for repost is 0.00001000 BTC (1000 Satoshi), your personal bonus – {js_bonus_amount} BTC (<b>{js_bonus_satoshi} Satoshi</b>)! Support the mining community by making a repost to Facebook and get a bonus on your account balance.</p>', {js_bonus_amount: bonusAmount, js_bonus_multiplier: bonusMultiplier, js_bonus_satoshi: bonusAmountSatoshi});
                            headerNotification.querySelector('.notification__title').innerHTML = createStringFromTemplate('Grab your personal repost bonus of <b>{js_bonus_amount} BTC for free</b>', {js_bonus_amount: bonusAmount});
                        } else {
                            //promoSharingPopup.querySelector('.popup__header .btc-bonus i').innerHTML = bonusAmount.toFixed(8);
                            promoSharingPopup.querySelector('.popup__body').innerHTML = '<h2>Support the mining community by making a repost to Facebook and get a bonus!</h2><p>Support the mining community by making a repost to Facebook and get a bonus of 0.00001000 BTC (1000 Satoshi) on your account.</p>';
                            headerNotification.querySelector('.notification__title').innerHTML = 'Grab your personal bonus of up to <b>0.00001000 BTC for free</b>';
                        }
                    }

                    if (hideHeaderNotification === null && allow) {
                        hideHeaderNotification = false;
                        headerNotification.style.display = 'block';
                    } else {
                        hideHeaderNotification = true;
                    }

                    if (hidePromoSharingPopup === null && allow) {
                        hidePromoSharingPopup = false;
                        promoSharingPopup.classList.add('active');
                        amountAnimation(document.querySelector('.popup__header .btc-bonus i'), 0, bonusAmount);
                    } else {
                        hidePromoSharingPopup = true;
                    }

                } else {
                    throw new Error();
                }
            } catch (error) {
                console.log(error)
            }
        }
    }

    function createStringFromTemplate(template, variables) {
        return template.replace(new RegExp("\{([^\{]+)\}", "g"), function(_unused, varName){
            return variables[varName];
        });
    }

    window.addEventListener('scroll', function(e) {
        if ( !hideHeaderNotification && (document.body.scrollTop > 150 || document.documentElement.scrollTop > 150) && window.innerWidth > 768 ) {
            page.classList.add('page_sticky-notification');
            page.style.paddingTop = headerNotification.offsetHeight+'px';
        } else {
            page.classList.remove('page_sticky-notification');
            page.style.paddingTop = '0'
        }
    });

    headerNotification.addEventListener('click', function(event) {
        window.location.href = 'https://cryptobrowser.site/en/promo/sharing/199125/?l=8';
    });

    document.querySelector('#headerNotification .close').addEventListener('click', function(event) {
        event.stopPropagation();
        closeHeaderNotification();
    });

    function closeHeaderNotification(event) {
        headerNotification.style.display = 'none';
        hideHeaderNotification = true;
        try {
            sessionStorage.setItem('hideHeaderNotification', hideHeaderNotification);
        } catch (e) {

        }
    }

    // Popup
    document.querySelector('#closePromoSharingPopup').addEventListener('click', function(event) {
        event.stopPropagation();
        closePromoSharingPopup();
    });

    document.querySelector('#getBonus').addEventListener('click', function(event) {
        hidePromoSharingPopup = true;
        try {
            localStorage.setItem('hidePromoSharingPopup', hidePromoSharingPopup);
        } catch (e) {
            
        }
    });

    function closePromoSharingPopup(event) {
        promoSharingPopup.classList.remove('active');
        hidePromoSharingPopup = true;
        try {
            localStorage.setItem('hidePromoSharingPopup', hidePromoSharingPopup);
        } catch (e) {
            
        }
    }

    function amountAnimation(element, startValue, endValue) {
        var numAnim = new CountUp(element, startValue, endValue, 8, 2);
        if (!numAnim.error) {
            numAnim.start();
        }
    }
</script>

