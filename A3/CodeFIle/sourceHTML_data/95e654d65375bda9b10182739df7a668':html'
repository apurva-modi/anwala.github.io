<script type="text/javascript">
    $(document).ready(function(){
        $( "#calculate" ).click(function(e) {
            e.preventDefault();
            var amount = $("#amount").val();
            var selectedval = $( "#convert_from" ).val();
            $.ajax({
                type: "POST",
                url: 'https://orbistransfer.com/account/conversion',
                data: {'convert_from' : selectedval,'amount' : amount},
                dataType: 'html',
                success: function(data){
                    console.log(data);
                    $('#calculate_conversion').html(data);
                }
            });
        });


    });
</script>


