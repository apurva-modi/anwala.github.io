 $("#bookmark").click(function() {
  var num = $(this).data("favonum");
  var target = $(this).data("favotarget");
  var button = this;
  if($(this).data('condition') == false){
    $.ajax({
      url: '/bookmark/add',
      type: 'POST',
      dataType: 'text',
      data: {target: target,id: num},
        success : function(data, textStatus, jqXHR){
            $(button).text($(button).data('text-clicked'));
            $(button).attr('onclick', 'location.href="/mypage/bookmark"');
            $(button).data('condition',true);
        },
        error: function(data, textStatus, jqXHR){
        }
    })
  }
});
</script>

