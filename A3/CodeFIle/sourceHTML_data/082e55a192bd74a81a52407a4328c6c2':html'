          category: 'Download',
          action: this._cleanUri(uri),
          label: id
        };
      } else {
        return {
          category: 'Outbound',
          action: this._cleanUri(uri),
          label: id
        };
      }
    };

    EventTracker.prototype._cleanUri = function(uri) {
      return uri.replace(/clk[n,g]\//, '').replace('/', '://');
    };

