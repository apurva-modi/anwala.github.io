          category: 'Email',
          action: uri.replace('mailto:', ''),
          label: id
        };
      } else if (uri.indexOf('/tel/') > -1) {
        return {
          category: 'Phone',
          action: uri.split('/tel/')[1],
          label: id
        };
      } else if (uri.indexOf('/rel/') > -1) {
        return {
          category: 'Lightbox',
          action: 'Lightbox',
          label: id
        };
      } else if (/\.(pdf|doc|docx|csv)/.test(uri)) {
