    EventTracker.prototype._linkHandler = function(event, $parentElm) {
      var $element, browser, hasHandler, id, params, parentId, preventingDefault, target, uri;
      if (!this._isGaLoaded()) {
        return;
      }
      $element = lp.jQuery(event.currentTarget);
