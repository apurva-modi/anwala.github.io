  <script>
    if (localStorage.getItem('user') && JSON.parse(localStorage.getItem('user')).settings.darkTheme) {
      document.documentElement.classList.toggle('theme-dark');
    }
    (function () {
      var OriginalDate = Date;
      Date = function () {
        var date;
        if (arguments.length === 0) {
          date = new OriginalDate();
        } else if (arguments.length === 1) {  //Cannot use OriginalDate.apply(this, arguments).  See http://stackoverflow.com/questions/181348/instantiating-a-javascript-object-by-calling-prototype-constructor-apply
          if (typeof arguments[0] == "string" && isNaN(new OriginalDate(arguments[0]))) {
            arguments[0] = arguments[0].replace(/-/g, "/");
          }
          date = new OriginalDate(arguments[0]);
        } else if (arguments.length === 2) {
          date = new OriginalDate(arguments[0], arguments[1]);
        } else if (arguments.length === 3) {
          date = new OriginalDate(arguments[0], arguments[1], arguments[2]);
        } else if (arguments.length === 4) {
          date = new OriginalDate(arguments[0], arguments[1], arguments[2], arguments[3]);
        } else if (arguments.length === 5) {
          date = new OriginalDate(arguments[0], arguments[1], arguments[2], arguments[3], arguments[4]);
        } else if (arguments.length === 6) {
          date = new OriginalDate(arguments[0], arguments[1], arguments[2], arguments[3], arguments[4], arguments[5]);
        } else if (arguments.length === 7) {
          date = new OriginalDate(arguments[0], arguments[1], arguments[2], arguments[3], arguments[4], arguments[5], arguments[6]);
        }

        return date;
      };

      Date.prototype = OriginalDate.prototype;
      Date.now = OriginalDate.now;
      Date.OriginalDate = OriginalDate;
      Date.UTC = OriginalDate.UTC;
      Date.parse = OriginalDate.parse;
    })();

    var coincodex = {
      all_coins : false,
      metadata : false,
      all_coins_loaded : 0,
      loading_coins : false,
      blocking_ads : undefined,
      location : "unknown"
    };

    if (typeof window !== 'undefined') {
      (function () {
        var xhr = new XMLHttpRequest();
        xhr.open('GET', "https://coincodex.com/apps/coincodex/cache/all_coins_packed.json?t=" + Math.round(Date.now() / (60 * 1000)) + "&" + window.location.hostname);
        xhr.onload = function () {
          coincodex.loading_coins = false;
          if (xhr.status === 200) {
            var data_packed = JSON.parse(xhr.responseText);
            coincodex.all_coins = data_packed.data.map(function (entry) {
              return data_packed.columns.reduce(function (prev, column, index) {
                if (column === "last_update" && entry[index] * 1000 > Date.now()) {
                  entry[index] = Date.now() / 1000;
                }
                prev[column] = entry[index];
                return prev;
              }, {});
            });
            coincodex.all_coins_loaded = Date.now();
          } else {
            console.error("ALL COINS LOAD FAILED");
          }
        };
        xhr.send();
      })();
    }
    /*(function () {
      var xhr = new XMLHttpRequest();
      xhr.open('GET', "https://coincodex.com/api/coincodex/get_metadata/?t=" + Math.round(Date.now() / (30 * 1000)));
      xhr.onload = function () {
        coincodex.loading_coins = false;
        if (xhr.status === 200) {
          coincodex.metadata = JSON.parse(xhr.responseText);
        } else {
          console.error("METADATA LOAD FAILED");
        }
      };
      xhr.send();
    })();*/
    (function () {
      var xhr = new XMLHttpRequest();
      xhr.open('GET', "https://coincodex.com/api/country_code");
      xhr.onload = function () {
        coincodex.loading_coins = false;
        if (xhr.status === 200) {
          coincodex.location = xhr.responseText;
        } else {
          console.error("LOCATION LOAD FAILED");
        }
      };
      xhr.send();
    })();
</script>

