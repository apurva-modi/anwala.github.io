<script type="text/javascript">
	function showData(moblet){

		$('.profile-photo img').attr('src',moblet.image);
		$('.moblet-name').html(moblet.name);
		$('#cid').val(moblet.cid);
		$('#startInMillis').val(moblet.start);
		$('#endInMillis').val(moblet.end);
		
		$('.moblet-name').ellipsis({
			row : 4,
			onlyFullWords:true,
		});
	}
	$(function() {
		
		$('.action-btn').click(function(){
			var emails=$('#modal-emails').val();
			var cid=$('#cid').val();
			var message=$('#email-comment').val();
			var start = $('#startInMillis').val();
			var end = $('#endInMillis').val();
			if(start != null && start != ""){
				cm.postRequest('/v1/moblet/action/shareClip',{'cid':cid,
					'emails':emails,
					'comment':message,
					'cs':start,
					'ce':end,}, function(r){
					if(r.status.code == 200){
						$('#modal-confirm-title').html("Thank you");
						$('#modal-confirm-message').html("Profile was shared");
	                	$('#alerts-success').modal('show');
	                	$('#modal-share-identity').modal('hide');
					}
					else{
						$('#modal-confirm-title').css('color', '#ff471a');
						$('#modal-confirm-title').html("Error While Sharing");
						$('#modal-confirm-message').html(r.status.message);
	                	$('#alerts-success').modal('show');
					}
				});
			}
			
			else {
				cm.getRequest('/v1/moblet/shareByEmail?cid='+cid+'&email='+
						''+emails+'&comment='+message, function(r){
					if(r.status.code == 200){
						$('#modal-confirm-title').html("Thank you");
						$('#modal-confirm-message').html("Profile was shared");
	                	$('#alerts-success').modal('show');
	                	$('#modal-share-identity').modal('hide');
					}
					else{
						$('#modal-confirm-title').css('color', '#ff471a');
						$('#modal-confirm-title').html("Error While Sharing");
						$('#modal-confirm-message').html(r.status.message);
	                	$('#alerts-success').modal('show');
					}
				});
			}
		});
	});
</script>
  
    </body>
</html>

				

