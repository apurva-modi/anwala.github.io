            (function(){
  function pingIbeat() {
      window._pg_endpt=(new Date()).getTime();
    var e = document.createElement('script');
    e.setAttribute('language', 'javascript');
    e.setAttribute('type', 'text/javascript');
    e.setAttribute('src',"https://ibeat.indiatimes.com/js/pgtrackingV9.js");
    document.body.appendChild(e);
  }
  if (TimesGDPR && TimesGDPR.common.consentModule.gdprCallback){
        TimesGDPR.common.consentModule.gdprCallback(function(data){ //Block for EU users
            if(!data.isEUuser){
                pingIbeat();
            }
        });
    }else{
        pingIbeat();
    }
    
    })();

	                    
	                    
	                    </script></body></html>