				let xhrJ = new XMLHttpRequest();
				//xhrJ.open('GET', '/jobs/wp-json/wp/v2/job-listings?per_page=3&status=publish');
				xhrJ.open('GET', '/jobs/jm-ajax/get_listings/?page=1&per_page=3&orderby=featured&order=DESC');
				//xhrJ.responseType = 'json';
				xhrJ.onload = function () {
					if (xhrJ.status === 200 && xhrJ.readyState === 4) {
						//console.log(xhrJ.responseText);
						data = JSON.parse(xhrJ.response);
						//console.log(data.html);
						resultsJobs.innerHTML += `${data.html}`;
					}
					};
					xhrJ.send();</script> <div class="btce-jobs-actions"> <a class="btce-button btce-button--small btce-button--has-icon-right btce-icon--forward" href="/jobs/">Alle Jobs</a> <a class="btce-button btce-button--politics btce-button--small" href="/jobs/unsere-pakete-und-preise/">Jobanzeige schalten</a></div></div></div></div><div class="btce-container btce-container--small btce-padding--medium"><div class="btce-container__row"><div class="btce-container__item__col-7 btce-container__border-top btce-container__border--primary"><h3 class="btce-container__headline">Aktuell</h3><div class="btce-article-listing btce-article-listing--vertical"><div class="btce-article-listing__item"><div class="btce-article-listing__image__wrapper"> <a href="https://www.btc-echo.de/krypto-und-traditionelle-maerkte-kw9-bitcoin-trotz-dump-an-der-spitze/"> <img width="242" height="136" src="data:image/gif;base64,R0lGODdhAQABAPAAAP///wAAACwAAAAAAQABAEACAkQBADs=" data-lazy-src="https://www.btc-echo.de/wp-content/uploads/2019/03/nach-dump-abgeprallt-bitcoin_556556278-242x136.jpg" class="btce-article-listing__image wp-post-image" alt="Krypto- und traditionelle Märkte KW9 – Bitcoin trotz Dump an der Spitze" title="Krypto- und traditionelle Märkte KW9 – Bitcoin trotz Dump an der Spitze" data-lazy-srcset="https://www.btc-echo.de/wp-content/uploads/2019/03/nach-dump-abgeprallt-bitcoin_556556278-242x136.jpg 242w, https://www.btc-echo.de/wp-content/uploads/2019/03/nach-dump-abgeprallt-bitcoin_556556278-366x207.jpg 366w, https://www.btc-echo.de/wp-content/uploads/2019/03/nach-dump-abgeprallt-bitcoin_556556278-189x106.jpg 189w, https://www.btc-echo.de/wp-content/uploads/2019/03/nach-dump-abgeprallt-bitcoin_556556278-433x244.jpg 433w, https://www.btc-echo.de/wp-content/uploads/2019/03/nach-dump-abgeprallt-bitcoin_556556278-224x126.jpg 224w, https://www.btc-echo.de/wp-content/uploads/2019/03/nach-dump-abgeprallt-bitcoin_556556278-240x134.jpg 240w, https://www.btc-echo.de/wp-content/uploads/2019/03/nach-dump