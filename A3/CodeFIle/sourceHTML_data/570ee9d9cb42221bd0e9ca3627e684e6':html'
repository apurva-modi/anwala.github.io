<script>

				var googletag = googletag || {};
				googletag.cmd = googletag.cmd || [];
				(function() {
				   var gads = document.createElement('script');
				   gads.async = true;
				   var useSSL = 'https:' == document.location.protocol;
				   gads.src = (useSSL ? 'https:' : 'http:') +
				       '//www.googletagservices.com/tag/js/gpt.js';
				   var node = document.getElementsByTagName('script')[0];
				   node.parentNode.insertBefore(gads, node);
				})();


				var gptAdSlots = [];
			  googletag.cmd.push(function() {

					var header_mapping = googletag.sizeMapping()
						.addSize([980, 0], [728, 90])
						.addSize([0, 0], [320, 50])
						.build();

          var middle_banner_mapping = googletag.sizeMapping()
  					.addSize([980, 0], [[970, 250], [300, 250]])
  					.addSize([0, 0], [300, 250])
  					.build();

					var catfish_right_mapping = googletag.sizeMapping()
						.addSize([740, 0], [400, 200])
						.addSize([0, 0], [200, 100])
						.build();

					var half_page_mapping = googletag.sizeMapping()
						.addSize([980, 0], [300, 600])
						.addSize([0, 0], [300, 250])
						.build();

					var takeover_mapping = googletag.sizeMapping()
				    .addSize([1024, 0], [800, 600])
				    .addSize([0, 0], [300, 200])
				    .build();

					var skins_mapping = googletag.sizeMapping()
				    .addSize([1550, 0], [120, 600])
						.addSize([0, 0], [])
				    .build();

					gptAdSlots.push(
						googletag.defineOutOfPageSlot('/38893584/brief_out_of_page', 'div-gpt-ad-1532258087830-0')
						.addService(googletag.pubads())
					);

					gptAdSlots.push(
						googletag.defineSlot('/38893584/brief_pixel', [1, 1], 'div-gpt-ad-1539242274776-0')
						.addService(googletag.pubads())
					);

				  gptAdSlots.push(
						googletag.defineSlot('/38893584/brief_main_leaderboard', [[728, 90], [320, 50]], 'div-gpt-ad-1532154308144-0')
						.defineSizeMapping(header_mapping)
						.addService(googletag.pubads())
						.setTargeting('category', ['blockchaincrypto'])
					);

				  	var isMobile = window.matchMedia("only screen and (min-width: 768px)");
					if (isMobile.matches) {

						gptAdSlots.push(
							googletag.defineSlot('/38893584/brief_main_rectangle', [300, 250], 'div-gpt-ad-1532160055782-0')
							.addService(googletag.pubads())
							.setTargeting('category', ['blockchaincrypto'])
						);

					} else {

						gptAdSlots.push(
							googletag.defineSlot('/38893584/brief_main_rectangle', [300, 250], 'div-gpt-ad-1532160055782-1')
							.addService(googletag.pubads())
							.setTargeting('category', ['blockchaincrypto'])
						);
					}

					gptAdSlots.push(
						googletag.defineSlot('/38893584/brief_skin_left', [120, 600], 'div-gpt-ad-1532172925849-0')
						.defineSizeMapping(skins_mapping)
						.addService(googletag.pubads())
						.setTargeting('category', ['blockchaincrypto'])
					);

					gptAdSlots.push(
						googletag.defineSlot('/38893584/brief_skin_right', [120, 600], 'div-gpt-ad-1532173151279-0')
						.defineSizeMapping(skins_mapping)
						.addService(googletag.pubads())
						.setTargeting('category', ['blockchaincrypto'])
					);

					var catfish_right_banner = googletag.defineSlot('/38893584/brief_catfish_right', [[400, 200], [200, 100]], 'div-gpt-ad-1532064391227-0')
				    	.defineSizeMapping(catfish_right_mapping)
				    	.addService(googletag.pubads())
							.setTargeting('category', ['blockchaincrypto']);
				  gptAdSlots.push(catfish_right_banner);

					var takeover_banner = googletag.defineSlot('/38893584/brief_takeover', [[300, 200], [800, 600]], 'div-gpt-ad-1532158625138-0')
				        .defineSizeMapping(takeover_mapping)
				        .addService(googletag.pubads())
								.setTargeting('category', ['blockchaincrypto']);
					gptAdSlots.push(takeover_banner);


					var root_path = "node";
					var node_type = "article";

					if(root_path == "") { // frontpage
						console.log("define slots for frontpage");

						gptAdSlots.push(googletag.defineSlot('/38893584/brief_fp_rectangle_1', [[970, 250], [300, 250]], 'div-gpt-ad-1532159918109-0')
              .defineSizeMapping(middle_banner_mapping)
							.addService(googletag.pubads())
							.setTargeting('category', ['blockchaincrypto'])
						);
						gptAdSlots.push(googletag.defineSlot('/38893584/brief_fp_rectangle_2', [300, 250], 'div-gpt-ad-1532160221427-0')
							.addService(googletag.pubads())
							.setTargeting('category', ['blockchaincrypto'])
						);
						gptAdSlots.push(googletag.defineSlot('/38893584/brief_fp_middle_large_banner', [[970, 250], [300, 250]], 'div-gpt-ad-1532013019138-0')
							.defineSizeMapping(middle_banner_mapping)
							.addService(googletag.pubads())
							.setTargeting('category', ['blockchaincrypto'])
						);
						gptAdSlots.push(googletag.defineSlot('/38893584/brief_fp_rectangle_3', [300, 250], 'div-gpt-ad-1540068214014-0')
							.addService(googletag.pubads())
							.setTargeting('category', ['blockchaincrypto'])
						);
					}
					else if((root_path == "node") && (node_type == "article")) { // article page
						console.log("define slots for article page");
						gptAdSlots.push(
							googletag.defineSlot('/38893584/brief_ap_leaderboard_1', [[320, 50], [728, 90]], 'div-gpt-ad-1546616116742-0')
							.defineSizeMapping(header_mapping)
							.addService(googletag.pubads())
							.setTargeting('category', ['blockchaincrypto'])
						);

						if (isMobile.matches) {

							gptAdSlots.push(googletag.defineSlot('/38893584/brief_ap_rectangle_1', [300, 250], 'div-gpt-ad-1532155805707-0')
								.addService(googletag.pubads())
								.setTargeting('category', ['blockchaincrypto'])
							);
							// gptAdSlots.push(
							// 	googletag.defineSlot('/21727363002/brief_ap_half_page', [[300, 600], [300, 250]], 'div-gpt-ad-1532155968276-0')
							// 	.defineSizeMapping(half_page_mapping)
							// 	.addService(googletag.pubads())
							// 	.setTargeting('category', ['blockchaincrypto'])
							// );
						} else {
							gptAdSlots.push(googletag.defineSlot('/38893584/brief_ap_rectangle_1', [300, 250], 'div-gpt-ad-1532155805707-1')
								.addService(googletag.pubads())
								.setTargeting('category', ['blockchaincrypto'])
							);
							// gptAdSlots.push(
							// 	googletag.defineSlot('/21727363002/brief_ap_half_page', [[300, 600], [300, 250]], 'div-gpt-ad-1532155968276-1')
							// 	.defineSizeMapping(half_page_mapping)
							// 	.addService(googletag.pubads())
							// 	.setTargeting('category', ['blockchaincrypto'])
							// );
						}

					}
					else if(root_path == "taxonomy") { // category page
						console.log("define slots for category page");
					}


					if((root_path == "taxonomy") || ((root_path == "node") && (node_type == "article"))) { // category OR article page (sponsorship ad)
						console.log("define slots for sponsorship");
						gptAdSlots.push(
							googletag.defineSlot('/38893584/brief_category_sponsorship', [320, 50], 'div-gpt-ad-1533022746863-0')
							.addService(googletag.pubads())
							.setTargeting('category', ['blockchaincrypto'])
						);
					}



					// add listener to position catfish and takeover once they are rendered
					googletag.pubads().addEventListener('slotRenderEnded', function(event) {
						//console.log(event);
						if((event.slot === takeover_banner) && !event.isEmpty ){
							console.log("slot rendered for takeover banner and banner is active");
							// position_takeover(event);
							//setTimeout( position_takeover(event), 10000 );
						}

						if ((event.slot === catfish_right_banner) && !event.isEmpty){
							position_catfish_right(event);
						}
				  });


			    googletag.pubads().enableSingleRequest();
					googletag.pubads().collapseEmptyDivs();
			    googletag.enableServices();


					function position_catfish_right(event){
						//console.log(event.size);
						var top = jQuery(window).height() - event.size[1];
						jQuery("#catfish-right-wrapper").css({
								"display" : "block",
								"z-index" : "99999",
								"position" : "fixed",
								"top" : top+"px",
								"right" : "0",
								"height" : event.size[1]+"px"
						});
					}


					function position_takeover(event){
						console.log("positioning takeover");
					  var width = event.size[0];
					  var height = event.size[1];
					  var margin_left = -(event.size[0]/2);
					  var margin_top = -(event.size[1]/2);

					  jQuery("#takeover-wrapper").css({

				    	"position" : "fixed",
				    	"top" : "0px",
				    	"left" : "0px",
			        "z-index" : "200000",
				    	"width" : jQuery(window).width()+"px",
				    	"height" : jQuery(window).height()+"px",
				    	"min-height" : jQuery(window).height()+"px",
				    	"background-color": "rgba(0,0,0,0.7)",
					  });
					  jQuery("#takeover-container").css({
				    	"display" : "block",
				    	"position" : "absolute",
				    	"top" : "50%",
				    	"left" : "50%",
				    	"width" : width+"px",
				    	"height" : height+"px",
				    	"margin-left" : margin_left+"px",
				    	"margin-top" : margin_top+"px"
					  });
						jQuery("#takeover-wrapper").fadeIn(1000);
						//setTimeout( 'jQuery("#takeover-wrapper").fadeIn(800);', 2000 );
					  setTimeout( 'jQuery("#takeover-wrapper").fadeOut(800);', 7000 );

				  }

			  });

			</script>


