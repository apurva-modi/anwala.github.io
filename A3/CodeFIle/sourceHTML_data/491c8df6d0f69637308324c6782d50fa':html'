#mvp-nav-menu ul li a,
#mvp-nav-menu ul li.menu-item-has-children a {
	padding-bottom: 26px;
	}

#mvp-nav-main {
	text-align: center;
	}

.mvp-fixed #mvp-nav-main {
	text-align: left;
	}

#mvp-nav-menu {
	display: inline-block;
	}

.mvp-fixed #mvp-nav-menu {
	display: block;
	}
		

	#mvp-post-content-mid {
		margin-top: -150px;
		}
		
