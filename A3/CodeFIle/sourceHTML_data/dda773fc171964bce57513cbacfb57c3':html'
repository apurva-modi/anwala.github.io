        $('#socialShare').on('show.bs.modal', function (event) {
            var button = $(event.relatedTarget); // Button that triggered the modal
            var recipient = button.data('airdrop'); // Extract info from data-* attributes
            var url = button.data('url'); // Extract info from data-* attributes
            // If necessary, you could initiate an AJAX request here (and then do the updating in a callback).
            // Update the modal's content. We'll use jQuery here, but you could use a data binding library or other methods instead.
            var modal = $(this);
            modal.find('.modal-title').text('Social Share for ' + recipient);
            modal.find('a').each(function (index, item) {
                var currentHref = $(item).attr('href');
                $(item).attr('href', currentHref + encodeURIComponent(url));
                // console.log(item);
            })
        })
    </script>
