    jQuery(document).ready(function ($) {
        $("input").keypress(function (e) {
            if (this.type != "button" && ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13))) {
                $('.default:visible')[0].click();
                return true;
            }
        });
    });

    
