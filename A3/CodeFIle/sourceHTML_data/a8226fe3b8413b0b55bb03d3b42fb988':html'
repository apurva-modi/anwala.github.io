                    resizable: true,
                    height: 456,
                    width: 895,
                    show: {
                        effect: "drop",
                        duration: "fast"
                    },
                    modal: true,
                    open: function(event, ui) {
                        $('.ui-widget-overlay').bind('click', function(event, ui) {
                            $('#divGameDetails').dialog('close');
                        });
                    }
                });
            }
