    // getCryptoCompareData();
    function extraNavIconSet(){
        var liItems = document.getElementById('child-extra-nav-wrap').getElementsByTagName('li');
        for (let i=0; i<liItems.length; i++) {
            var item = liItems[i].getElementsByTagName("i");
            if(item.length>0){
                liItems[i].classList.add("new-li");
            }
            var link = liItems[i].getElementsByTagName("span");
            if(link.length>0){
                liItems[i].classList.add("new-link");
            }
        }
    }
    extraNavIconSet();
