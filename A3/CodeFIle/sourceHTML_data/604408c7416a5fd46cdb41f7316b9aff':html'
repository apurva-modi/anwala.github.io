  	var postUrlHistory = [];
	var nextPostUrl;
	function findNextPostUrl(){
	var prevElem = jQuery(".mh-post-nav-prev a");
	var prevHref = jQuery(prevElem[prevElem.length - 1]).attr("href");
	var nextElem = jQuery(".mh-post-nav-next a");
	var nextHref = jQuery(nextElem[nextElem.length - 1]).attr("href");
	if(postUrlHistory.indexOf(prevHref) >= 0) {
		nextPostUrl = nextHref;
	}else {
		postUrlHistory.push(prevHref);
		nextPostUrl = prevHref;
	}
	}
	findNextPostUrl();
	var container = jQuery('.content-mh').infiniteScroll({
	path : function () {
		return nextPostUrl;
	},
	append: 'article',
	history: 'push',
	historyTitle: true,
	status: ".page-load-status",
	scrollThreshold: 200
	});
	container.on('append.infiniteScroll', function () {
	findNextPostUrl();
	enlargeListener();
	var loaders = jQuery(".loader-ellips");
	for (var i = 0; i < loaders.length - 1; i++)
		jQuery(loaders[i]).css('display', 'none');
		jQuery("article.post.hentry:not(:first-child").each(function () {
	});


