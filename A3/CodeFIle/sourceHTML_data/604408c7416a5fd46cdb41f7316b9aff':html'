  function submitEmForm(formClass){
    var txt = jQuery('.'+formClass+' button').html();
    jQuery('.'+formClass+' button').html('<i class="fa fa-spinner fa-spin fa-forward"></i>');
    jQuery.ajax({
        url: 'https://api.coingape.com/home/mailing_list_subscribe',
        dataType: 'json',
        type: 'post',
        data: {
          "email": jQuery('.'+formClass+' input').val()
        },
        success: function( data, textStatus, jQxhr ){
          jQuery('.'+formClass+' .resp').html('<span style="color:green">You are successfully subscribed.</span>');
          jQuery('.'+formClass+' button').html(''+txt+'');
        },
        error: function( jqXhr, textStatus, errorThrown ){
          jQuery('.'+formClass+' .resp').html('<span style="color:orange">You are already subscribed.</span>');
          jQuery('.'+formClass+' button').html(''+txt+'');
        }
    });
  }
</script>
