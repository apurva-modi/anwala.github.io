<script>
    jQuery('.modalx-content .close').click(()=>{customPopToggle(true)})
    function customPopToggle(hide=false){
        hide ? jQuery('.modalx').css({display: 'none'}) : jQuery('.modalx').css({display: 'block'});
        if(hide){
            localStorage.setItem('pop_action','denied');
        }
    }
    jQuery('#menu-item-11716').click(()=>{
        customPopToggle();
    })
    jQuery(document).ready(()=>{
        
        jQuery("#social-side .social-text").hover(function(){
            var icon = jQuery(this).find('i');
            jQuery(this).css({background:icon.css('color')});
            icon.css({color:'#fff'});
        },function(){
            var back = jQuery(this).css('backgroundColor');
            var icon = jQuery(this).find('i').css({color:back});
            jQuery(this).css({background:'#fff'});
        });
        jQuery('body').click(()=>{
            if(isLinkOpen){
                jQuery('.link-box').css({display:'none'});
                isLinkOpen = false;
                jQuery('.wh').css({opacity:'0.4'});
            }
        })
        jQuery('#myModalx').click(()=>{
            // customPopToggle(true);
        })
    })

    var divs = {
        eth:{},
        btc:{},
        ltc:{},
        xrp:{}
    };
    function setB(){
        var tickerMap = {
            BTC:0,
            LTC:2,
            ETH:1,
            XRP:3
        }
        for(let i in divs){
            jQuery(jQuery('.link-box')[tickerMap[i]]).append(`
                <div class="fiat-x"><a href="https://www.cryptocompare.com/" rel="nofollow"><i class="fa fa-${'eur'}"></i> ${divs[i]['EUR']['PRICE'].toFixed(3)}</a></div>
                <div class="fiat-x"><a href="https://www.cryptocompare.com/" rel="nofollow"><i class="fa fa-${'gbp'}"></i> ${divs[i]['GBP']['PRICE'].toFixed(3)}</a></div>
                <div class="fiat-x"><a href="https://www.cryptocompare.com/" rel="nofollow"><i class="fa fa-${'inr'}"></i> ${divs[i]['INR']['PRICE'].toFixed(3)}</a></div>
            `)
        }
    }
    function appendToDom(price,currency,ticker){
        price > 9999 ? price = price.toFixed(2) : price = price.toFixed(4) ;
        
        divs[ticker][currency] = price;
    }

    var isLinkOpen = false;
    function showLinkBox(index,ev){
        if(isLinkOpen){
            jQuery('.link-box').css({display:'none'});
            jQuery('.wh').css({opacity:'0.6'});
        }
        document.getElementsByClassName('link-box')[index].style.display = 'block';
        document.getElementsByClassName('wh')[index].style.opacity = '1';
        isLinkOpen = true;
        ev.preventDefault();
        ev.stopPropagation();
    }

    function setTickers(map){
        var tickerMap = {
            btc:0,
            ltc:2,
            eth:1,
            xrp:3
        }
        for(let i in map){
            if(map[i]['change']>=0){
                var c = '<i style="margin-right: 2px" class="fa fa-caret-up"></i> '+map[i]['change'];
                var color = '#2cef2f';
            }
            else{
                var c = '<i style="margin-right: 2px" class="fa fa-caret-down"></i> '+(-1*map[i]['change']);
                var color = '#fd8e3c';
            }
            jQuery('#'+map[i]['ticker']+'-price').html('$ '+map[i]['live_price']);
            jQuery('#'+map[i]['ticker']+'-perc').html(c+'%');
            jQuery('#'+map[i]['ticker']+'-perc').css({color});
            jQuery(jQuery('.link-box')[tickerMap[map[i]['ticker']]]).append(`
                <div class="fiat-x"><a href="https://www.cryptocompare.com/" rel="nofollow" target="_blank"><i class="fa fa-euro"></i> ${map[i]['quotes']['eur'].toFixed(3)}</a></div>
                <div class="fiat-x"><a href="https://www.cryptocompare.com/" rel="nofollow" target="_blank"><i class="fa fa-gbp"></i> ${map[i]['quotes']['gbp'].toFixed(3)}</a></div>
                <div class="fiat-x"><a href="https://www.cryptocompare.com/" rel="nofollow" target="_blank"><i class="fa fa-inr"></i> ${map[i]['quotes']['inr'].toFixed(3)}</a></div>
            `);
        }
    }

    function getCoinGeckoData(){
        let map = {
            bitcoin:{ticker:'btc'},
            ripple:{ticker:'xrp'},
            ethereum:{ticker:'eth'},
            litecoin:{ticker:'ltc'},
        }

        let coinGeckoUrl = "https://api.coingecko.com/api/v3/simple/price?ids=bitcoin%2Clitecoin%2Cripple%2Cethereum&vs_currencies=usd%2Cgbp%2Ceur%2Cinr";

        let xHttpGecko = new XMLHttpRequest();
        let count = 0;
        xHttpGecko.onreadystatechange = function(){
            if(this.readyState==4 && this.status==200){
                let resp = JSON.parse(xHttpGecko.responseText);
                for(let i in resp){
                    var url = "https://api.coingecko.com/api/v3/coins/"+i+"/market_chart?vs_currency=usd&days=1";
                    let req = new XMLHttpRequest();
                    req.onreadystatechange = function(){
                        if(this.readyState==4 && this.status==200){
                            let respChart = JSON.parse(req.responseText).prices;

                            let change = (respChart[respChart.length-1][1]-respChart[0][1])/respChart[0][1];
                            // console.log(respChart[0][1])
                            map[i]['change'] = (change*100).toFixed(2);
                            map[i]['quotes'] = resp[i];
                            map[i]['live_price'] = respChart[respChart.length-1][1].toFixed(2);
                            count++;
                            if(count==4){
                                setTickers(map);
                            }
                        }
                    }
                    req.open("GET", url, true);
                    req.send();
                }
            }
        }
        xHttpGecko.open("GET", coinGeckoUrl, true);
        xHttpGecko.send();
    }

    getCoinGeckoData();

    // function getCryptoCompareData(){
