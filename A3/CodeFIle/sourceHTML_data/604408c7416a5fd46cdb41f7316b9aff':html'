<script>
	function showGrowl(){
		if(screen.width > 1200){
			let userOption;
			try{
				userOption = localStorage.getItem('bookmark');
			}
			catch{
			}
			if(!userOption){
				if(navigator.platform.toUpperCase().indexOf('MAC')>=0){
					var txt = 'Command <span style="font-size:11px">(⌘)</span> + D';
				}
				else{
					var txt = 'Ctrl + D';
				}
				var html = '<span>Press </span><span style="font-weight:500">'+txt+'</span><span> to add this page to your bookmark</span>';
				var doc = '<div id="growl"><div class="cross text-right">&#x2715;</div><div class="text">'+html+'</div></div>';
				jQuery('body').prepend(doc);
				jQuery('#growl').fadeIn();
				jQuery('#growl').find('.cross').click(function(){
					try{
						localStorage.setItem('bookmark','denied');
					}
					catch{

					}
					jQuery('#growl').fadeOut();
				})
			}
		}
	}
	function appendCT(){
		var clone = jQuery('.ct-code').first().clone();
