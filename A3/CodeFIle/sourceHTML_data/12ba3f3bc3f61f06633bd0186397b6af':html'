<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
  function add_widget() {
    var header = document.querySelector('div[class*="td-header-wrap"]');
    if (header) {      
      var widget = document.createElement('iframe');
      var wdiv = document.createElement('div');
      
      widget.src = 'https://portaldobitcoin.com/indice/widget';
      widget.style.height = '60px';
      widget.style.width = '100%';
      widget.style.border = 'none';
      
      wdiv.style.maxWidth = '1400px';
      wdiv.style.margin = '0 auto';
      wdiv.style.border = 'none';
      wdiv.style.overflow = 'auto';
      wdiv.style.display = 'block';
      wdiv.style.position = 'relative';
      wdiv.style.top = '6px';
      
      wdiv.appendChild(widget);
      
      header.parentNode.insertBefore(wdiv, header.nextSibling);
    }
  }
  
  var ifHeight = 1150;
  var ifElement = document.querySelector('div.td-fix-index > iframe');
  function resize_index() {
    var paths = ['/cotacao-bitcoin/', '/cotacao-ethereum/', '/cotacao-litecoin/', '/cotacoes/'];
    if (paths.indexOf(window.location.pathname) >= 0) {
      var contentWrap = document.querySelector('div.tdc-content-wrap > div.tdc-row:first-of-type');
      if (!contentWrap) {
        contentWrap = document.querySelector('div.td-container.tdc-content-wrap');
      }
      if (contentWrap) {
        contentWrap.style.width = 'unset';
        contentWrap.style.maxWidth = '1400px';
      }
      
      setInterval(function(){
        var docHeight = ifElement.contentDocument.documentElement.offsetHeight;
        if (docHeight > ifHeight) {
          ifHeight = docHeight;
          ifElement.style.minHeight = docHeight + 'px';
        }
      }, 1000);
    }
  }  
  
  function pdbtc_wnd_load() {
    add_widget();
    resize_index();
  }
  
  document.addEventListener('DOMContentLoaded', pdbtc_wnd_load, false);  
  
  window.addEventListener('load', () => {

    document.getElementById('nexo-loan-btn').addEventListener('click', () => {
      const pop = document.getElementById('nexo-loan-popover');
      pop.style.display = pop.style.display === 'none' ? 'inline-block' : 'none';
    });
    document.body.addEventListener('click', (e) => {
      if (e.target.id === 'nexo-loan-btn') { return; }
      const pop = document.getElementById('nexo-loan-popover');
      pop.style.display = 'none';
    });

  });
</script>

