            <!-- pagefair -->
            <script type="text/javascript">
            (function() {
                function async_load(script_url){
                    var protocol = ('https:' == document.location.protocol ? 'https://' : 'http://');
                    var s = document.createElement('script'); s.src = protocol + script_url;
                    var x = document.getElementsByTagName('script')[0]; x.parentNode.insertBefore(s, x);
                }
                bm_website_code = '5B0A03E177304A78';
                jQuery(document).ready(function(){async_load('asset.pagefair.com/measure.min.js')});
            })();
        </script>
            <script type="text/javascript">
 	$(function() {
        var randomHash = function() {
            return (new Date()).getTime() + '' + Math.random()
        }

        var mobileAdContainer = document.getElementById('mobile-gallery');
        var desktopAdContainer1 = document.getElementById('desktop-ad1');
        var desktopAdContainer2 = document.getElementById('desktop-ad2');
        var adLoaded = {
            'mobile' : false,
            'desktop1' : false,
            'desktop2' : false
        }
        var hash = {
            'mobile' : randomHash(),
            'desktop1' : randomHash(),
            'desktop2' : randomHash()
        }
        var adTimeout = {
            'mobile' : undefined,
            'desktop1' : undefined,
            'desktop2' : undefined
        };

        var observeDOM = (function() {
            var MutationObserver = window.MutationObserver || window.WebKitMutationObserver,
                eventListenerSupported = window.addEventListener;

            return function(obj, callback){
                if (MutationObserver){
                    // define a new observer

                    var obs = new MutationObserver(function(mutations, observer) {
                        if (mutations[0].addedNodes.length || mutations[0].removedNodes.length) {
                            callback();
                        }
                    });
                    // have the observer observe foo for changes in children
                    obs.observe(obj, { childList: true, subtree: true });
                } else if (eventListenerSupported) {
                    obj.addEventListener('DOMNodeInserted', callback, false);
                    obj.addEventListener('DOMNodeRemoved', callback, false);
                }
            }
        })();

        $('#gallery-slider').on('swipe', function(slick, direction){
            galleryAd();
            currentSlide = parseInt($('#gallery-slider').slick('slickCurrentSlide')) + 1;
            pushGalleryToAnalytics(currentSlide);
        });

        $('#gallery-slider .slick-arrow').on('click', function(){
            galleryAd();
            currentSlide = parseInt($('#gallery-slider').slick('slickCurrentSlide')) + 1;
            pushGalleryToAnalytics(currentSlide);
        });

        function galleryAd() {
            var types = ['mobile', 'desktop1', 'desktop2'];
            var type;
            for(var i = 0; i < types.length; i++ ) {
                type = types[i];
                if (!adLoaded[type]) {
                    continue;
                }
                hash[type] = randomHash();
                createAd[type]();
                adLoaded[type] = false;
            }
        }

        function pushGalleryToAnalytics(slideNr) {
            var url = location.pathname.replace(".html", "");
            var newUrl = url + '-' + slideNr + '.html';
            ga('send', 'pageview', newUrl);
        }

        var onDomChange = function(type, container) {
            var images = Array.prototype.slice.call(container.getElementsByTagName('img'));
            var iframes = Array.prototype.slice.call(container.getElementsByTagName('iframe'));
            var elements = images.concat(iframes);
            for (var i = 0; i < elements.length; i++) {
                elements[i].onload = onElementLoaded.bind(null, hash[type], type, container);
            }
        };

         // Check width and height on subelement load to see if we need to push up
        var onElementLoaded = function(originHash, type, container) {
            if (originHash !== hash[type]) {
                return;
            }
            var width = container.offsetWidth;
            var height = container.offsetHeight;


            // Has no height or width, exit function
            if (width < 100 && height < 50) {
                console.log('No height or width detected, exiting', width, height);
                return;
            }

            if (!adTimeout[type]) {
                adTimeout[type] = setTimeout(function(){
                    adLoaded[type] = true;
                    adTimeout[type] = undefined;
                }, 1700);
            }


        };

        var createAd = {
            'mobile' : function(){
                            },
            'desktop1' : function(){
                                console.log("Desktop 1");
            },
            'desktop2' : function(){
                                console.log("Desktop 2");
            }
        }

        if (mobileAdContainer &&  desktopAdContainer1 && desktopAdContainer2) {
            observeDOM(mobileAdContainer, onDomChange.bind(null, 'mobile', mobileAdContainer));
            observeDOM(desktopAdContainer1, onDomChange.bind(null, 'desktop1', desktopAdContainer1));
            observeDOM(desktopAdContainer2, onDomChange.bind(null, 'desktop2', desktopAdContainer2));
        }
        
	});
</script>
