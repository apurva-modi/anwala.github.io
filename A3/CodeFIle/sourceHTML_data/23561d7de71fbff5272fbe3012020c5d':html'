jQuery(function ($) {
	$('input,textarea').on('blur', function () {
		if ($(this).val() !== '') { //assuming the form doesn't have some fields populated by default.
			$(this).addClass('ccValidInput');
			$(this).removeClass('ccInValidInput');
		} else {
			$(this).removeClass('ccValidInput');
			$(this).addClass('ccInValidInput');
		}
	});
	$('body.page-id-27 #gform_1 #input_1_2, body.page-id-5938 #gform_wrapper_5 #input_5_2, body.page-id-8136 #gform_wrapper_6 #input_6_3,tve_lg_input_container input').on('blur', function () {
		var testEmail = /^[A-Z0-9._%+-]+@([A-Z0-9-]+\.)+[A-Z]{2,4}$/i;
		if (testEmail.test($(this).val())) {
			$(this).addClass('ccValidInput');
			$(this).removeClass('ccInValidInput');
		} else {
			$(this).removeClass('ccValidInput');
			$(this).addClass('ccInValidInput');
		}
	});
});



