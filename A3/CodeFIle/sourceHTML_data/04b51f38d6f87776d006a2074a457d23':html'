function memberbenefits_menu(variant){
  document.getElementById("c-site-header-newsletters-button").innerHTML = "Member Benefits";
  document.getElementById("c-site-header-newsletters-button").href = "#";
  document.getElementById("c-site-header-newsletters-button").setAttribute('onclick', 'memberbenefits("' + variant + '")')
  document.getElementById("c-site-header-newsletters-button").setAttribute("data-analyticsclick", '{"type":"button","feature":"header","label":"subscribers only", "action":"see member benefit site","position":1,"hierarchy":1}');
  var memberb_links = document.getElementById('memberbenefits_header').getElementsByTagName('a');
  // custom links to each variant
  for (var i = 0; i < memberb_links.length ; i++){
    memberb_links[i].setAttribute('href', memberb_links[i].href + '_' + variant )
  }
}
    }]);
    
  }
  window.tgam.datalayer.ready.push(dwExecute);
})();
