  // global variables
  const notIE  = !(window.navigator.userAgent.indexOf("MSIE ") > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))

  // get cookie function read the value of specific cookie
  function getCookie(cname) {
      var name = cname + "=";
      var decodedCookie = decodeURIComponent(document.cookie);
      var ca = decodedCookie.split(';');
      for (var i = 0; i < ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0) == ' ') {
              c = c.substring(1);
          }
          if (c.indexOf(name) == 0) {
              return c.substring(name.length, c.length);
          }
      }
      return null;
  }

  // function to hide subscription pencil
  function hidearcpencil() {
      var styleNode = document.createElement("style");
      var styleText = document.createTextNode("#subscription-pencil-area { display:none  !important; }");
      styleNode.setAttribute("id", "hidepencil");
      styleNode.type = "text/css";
      styleNode.appendChild(styleText);
      document.getElementsByTagName("head")[0].appendChild(styleNode);
  }

  // hide subscription pencil on pagebuilder mode
  if(window.location.href.indexOf('.tgam.arcpublishing.com') > -1){
    hidearcpencil();
  }

    /**
   * Detect if the browser is running in Private Browsing mode
   *
   * @export
   * @returns {Promise}
   */
  function isPrivateMode() {
    return new Promise(function (resolve) {
      const on = function (){ return resolve(true);}// is in private mode
      const off = function(){ return resolve(false);} // not private mode
      const testLocalStorage = function () {
        try {
          if (localStorage.length) off();
          else {
            localStorage.x = 1;
            localStorage.removeItem('x');
            off();
          }
        } catch (e) {
          // Safari only enables cookie in private mode
          // if cookie is disabled then all client side storage is disabled
          // if all client side storage is disabled, then there is no point
          // in using private mode
          navigator.cookieEnabled ? on() : off();
        }
      };
      // Chrome & Opera
      if (window.webkitRequestFileSystem) {
        return void window.webkitRequestFileSystem(0, 0, off, on);
      }
      // Firefox
      if ('MozAppearance' in document.documentElement.style) {
        const db = indexedDB.open('test');
        db.onerror = on;
        db.onsuccess = off;
        return void 0;
      }
      // Safari
      const isSafari = navigator.userAgent.match(/Version\/([0-9\._]+).*Safari/);
      if (isSafari) {
        const version = parseInt(isSafari[1], 10);
        if (version >= 11) {
          try {
            var dbname = "hasDB";
            window.openDatabase(dbname, "1.0", dbname, 0);
            return off();
          } catch (_) {
            return on();
          };
        } else if (version < 11) {
          return testLocalStorage();
        }
      }
      // if (/constructor/i.test(window.HTMLElement)) {
      //   return testLocalStorage();
      // }

      // IE10+ & Edge
      if (!window.indexedDB && (window.PointerEvent || window.MSPointerEvent)) {
        return on();
      }
      // others
      return off();
    });
  }

  // set local storage cookie if incognito
  if(notIE){
    isPrivateMode().then(function (isPrivate) {
      localStorage.setItem('isPrivate', isPrivate);
    });
  }

  // AdBlock check
  function isAdBlocked(){
      var adElement = document.getElementsByClassName('c-ad')[0];
      var isadblock = adElement ? !( adElement.offsetWidth || adElement.offsetHeight || adElement.getClientRects().length ) : true;
      return isadblock;
  }

  // set darwin variant to ensure no variant switching
  function setdarwinVariant(testid, variant){
      localStorage.setItem('Dw__' + testid + '|global', variant);
  }

  // hide the pencil or the element
  function hideUnit(testid, pencilid) {
   setDarwinState(testid, 'hasClosed', true);
   document.getElementById(pencilid).className = 'u-hidden';
   event.target.setAttribute("aria-expanded", false);
  }

  // grab the number of times the users has seen the units and increment it by 1
  function setUnitState(testid, variant) {
     const darwinState = getDarwinState() || {};
     var hasSeenUnit = 1;
     if (darwinState.hasOwnProperty(testid) && darwinState[testid]['hasSeenUnit'] !== undefined) {
         hasSeenUnit += darwinState[testid]['hasSeenUnit'];
     }

     if(hasSeenUnit == 1)
     setDarwinState(testid, 'variant', variant);
     setDarwinState(testid, 'hasSeenUnit', hasSeenUnit);
  }

  // right now doesnt do anything much
  // but this can be useful if we want to define properties that cannot be changed
  // or seal the object in order to not delete keys.
  function DarwinState(key, value) {
      this.Key = key;
      this.Value = value;
  }

  // check darwin test local storage exist
  function getDarwinState() {
      const darwinTests = localStorage.getItem('tgam.darwin_tests');
      if (darwinTests !== null ) {
          return JSON.parse(darwinTests)
      }
      return null;
  }

  // set darwin local storage values
  function setDarwinState(testName, key, value) {
      var darwinState = JSON.parse(localStorage.getItem('tgam.darwin_tests')) || {};

      if (darwinState[testName] !== undefined) {
          darwinState[testName][key] = value;
      }
      else {
          const myTestState = new DarwinState(key, value);
          darwinState[testName] = {};
          darwinState[testName][myTestState.Key] = myTestState.Value;

      }
      localStorage.setItem('tgam.darwin_tests', JSON.stringify(darwinState));
  }

  // get province or city according to datalayer postal code
  function getprovince_city(){
    var pCodeFirstChar="";
    if(window.tgam.datalayer && window.tgam.datalayer.identity && window.tgam.datalayer.identity.postal){
      pCodeFirstChar = window.tgam.datalayer.identity.postal.substr(0, 1).toUpperCase();
      if(pCodeFirstChar == 'V'){
        return 'BC';
      }else if(pCodeFirstChar == 'T'){
        return 'AB';
      }else if (pCodeFirstChar == 'L' || pCodeFirstChar == 'M'){
        return 'ON';
      }else if (pCodeFirstChar == 'K'){
        return 'OT';
      }else if (pCodeFirstChar == 'B'){
        return 'HF';
      }else if (pCodeFirstChar == 'G' || pCodeFirstChar == 'H'){
        return 'QC';
      }else{
        return '';
      }
    }else{
      return pCodeFirstChar;
    }
  }

  // count the number of views for homepage, for custom units
  if(window.location.href === 'https://www.theglobeandmail.com/'){
    var count = getCookie('homepgfeature') ? Number(getCookie('homepgfeature'))+1:1;
    count = count > 15 ? 0 : count;
    document.cookie = 'homepgfeature='+ count;
  }

  // schedule the test
  function schedule(start, end){
        var today= new Date();
        return (new Date(start) <= today) && (today <= new Date(end));
  }

  // set number of views
  function setFrequency(pencilid){
    console.log(getCookie(pencilid));
    if (getCookie(pencilid) !== "true") {
      var count = getCookie(pencilid)==null? 1: parseInt(getCookie(pencilid))+1;
      document.cookie = pencilid + "=" + count + ";path=/";
    }
  }

  // call apis
 function getapi(url, compare){
    if (window.navigator.userAgent.indexOf("MSIE ") === -1) {
     return fetch(url)
     .then(function (resp) {
         return resp.json();
     })
     .then(function (data) {
       if (url.indexOf('anonymoususerprofile')>-1){
         return getWesternProvince(data.Features.Today.Province, compare);
       // readinghistory
       }else if (url.indexOf('regpreuserprofile')>-1){
         // return getAuthor(data.readingHistoryArticles, compare)
         if (compare[2].indexOf('article')>-1){
           //CEM-1014 for check
            return getArticle(data, compare);
         } else {
           //CEM-1020 for check
            return getArticle_politics(data, compare);
         }
       }
     })
     .catch(function (error) {
         return error;
     });
    } else {
      return false;
    }
  }

  // Anonymous user profile functions
  function getCountry(object, countryCode){
    //temporary target UK
    if (countryCode === 'GB'){
      if( object !== 'US' && object !== 'CA'){
        customElement();
      }
    }
    if( object === countryCode){
      customElement();
    }
    return;
  }

  function checkCountry(object, countryCode){
    if( object === countryCode){
      analyticsLayer.push({
                  "event": "clickEvent",
                  "clickEvent": {
                      "testId": "cem111",
                      "feature": "checkCountry",
                      "action": countryCode,
                      "type": "test",
                      "label": "country",
                      "variantName": "none",
                      "segmentName": "none"
                  }
              });
    }
    return;
  }
</script></div> </div> </div> <div class="u-wrapper pb-feature pb-layout-item pb-f-global-header" id="fRqlNc2lan7ujr"> <header class="c-site-header c-site-header--article c-site-header--article--buttons-hidden c-site-header--sticky"> <div class="c-site-header__top"> <div class="c-site-header__top-content"> <div class="c-site-header__hamburger"> <button type="button" id="js-big-mac" class="c-button c-button--dark-primary c-button--no-border c-button--icon-only" aria-label="Open menu panel" aria-controls="site-header-menu-panel" aria-expanded="false"> <svg class="c-button__icon" role="img" pointer-events="none" focusable="false" aria-hidden="true"> <use xlink:href="/pb/resources/assets/img/tgam-patterns/sprite.svg?_=3_0_20#menu"></use> </svg> </button> </div> <div class="c-site-header__logo"> <a href="/" class="c-site-header__logo-link" data-analyticsclick='{"type":"link","feature":"header","label":"the globe and mail logo","position":1,"hierarchy":1}'> <svg class="c-site-header__logo-art" pointer-events="none" focusable="false" aria-hidden="true"> <use xlink:href="/pb/resources/assets/img/tgam-patterns/sprite.svg?_=3_0_20#tgam-logo-boxed"></use> </svg> <h2 class="c-site-header__logo-text u-visually-hidden">The Globe and Mail</h2> </a> </div> <div class="c-site-header__text"> <h3 class="c-site-header__text-wrapper"> <span class="c-site-header__text-content c-site-header__text-content--section"> <a href="/business/" class="c-site-header__text-link">Report on Business</a> </span> <span class="c-site-header__text-content c-site-header__text-content--headline">How did QuadrigaCX work? A guide to complicated crypto trading</span> </h3> </div> <div class="c-site-header__buttons"> <div class="c-site-header__button-group c-site-header__button-group--primary"> <a href="/newsletters/" id="c-site-header-newsletters-button" class="c-button c-button--no-border c-button--small u-hidden js-role-visibility--unlimited" data-analyticsclick='{"type":"button","feature":"header","label":"