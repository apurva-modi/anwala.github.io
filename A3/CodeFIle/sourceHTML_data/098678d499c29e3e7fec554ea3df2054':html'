    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.9/angular.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.6.9/angular-sanitize.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.21.0/moment.js"></script>
    <script src="/scripts/jquery/dist/jquery.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.0/clipboard.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment-timezone/0.5.14/moment-timezone.min.js"></script>
    <script src="//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-2.5.0.js"></script>
    <script src="/scripts/angular-ui-router/release/angular-ui-router.js"></script>
    <script src="/scripts/angular-toastr/dist/angular-toastr.tpls.js"></script>
    <script>
        angular.module('cryptonews',['ui.router','ngSanitize','toastr'])
            .config(function ($stateProvider,$locationProvider) {
                $locationProvider.html5Mode({
                    enabled: true,
                    requireBase: false,
                    rewriteLinks: false
                });
                $stateProvider
                    .state('home.news', {
                        url: '/news/:newstitle',
                        templateUrl: './js/news/newsdetails/newsdetails.html',
                        controller: 'newsDetailsCtrl'
                    });
            })
            .controller('cryptoctrl',function($scope,$http,$stateParams,$state,$location,toastr) {
                $("div")[0].style = undefined;
                new ClipboardJS('.cpy-url');
                function strip(html)
                {
                    let tmp = document.createElement("DIV");
                    tmp.innerHTML = html;
                    return tmp.textContent || tmp.innerText || "";
                }
                $scope.mobileDevice = false;
                $scope.newsData = {};
                if(screen.width < 500 ||
                    navigator.userAgent.match(/Android/i) ||
                    navigator.userAgent.match(/webOS/i) ||
                    navigator.userAgent.match(/iPhone/i) ||
                    navigator.userAgent.match(/iPod/i)) {
                    $scope.mobileDevice = true;
                }
                $scope.page = 1;
                $scope.Types = {newsType:'latest'};
                $scope.newsDatas =[{
                    imageUrls: ["https://i.postimg.cc/nr8jksST/131118080832-china-bitcoin-640x360.jpg"],
                    title: "Chinese Crypto Miner Predicts That Bitcoin Could Reach $740K",
                    feed: {link:"https://cointelegraph.com/news/chinese-crypto-miner-predicts-that-bitcoin-could-reach-740k",name: "Cointelegraph"},
                    _id: "5c6760fcfa8d6f53ecfa13b3",
                    tags: "china,crypto".split(','),
                    content: "Zhu Fa, co-founder of Poolin, a Chinese-based crypto mining pool, predicted that the Bitcoin (BTC) price could hit 5 million Chinese yuan ($738,000 (USD). While Zhu noted that “it now feels more like a bear market,” he reportedly predicted that in the next bull run, prices will be 10–20 times higher than previous ones. Zhu also noted that massive prices spikes like the one that resulted in the $20,000 per BTC high in 2017, will not always exist, adding that the next bull run could be the last.",
                    newsUrl: "chinese-crypto-miner-predicts-that-bitcoin-could-reach-740k",
                    toBePublishedOn:"Sat Feb 16 2019 00:49:00 GMT+0000 (UTC)",
                    link:"https://cointelegraph.com/news/chinese-crypto-miner-predicts-that-bitcoin-could-reach-740k",
                    Type: ""
                }];
                $scope.newsDatas[0].title = strip($scope.newsDatas[0].title);
                $scope.newsDatas[0].content = strip($scope.newsDatas[0].content);
                $http.get('/feed/banner')
                    .then((response) => {
                        $scope.bannerId = response.data._id;
                        let index = Math.floor(Math.random() * (response.data.imageUrls.length));
                        $scope.bannerUrl = response.data.imageUrls[index];
                        $scope.bannerLink = response.data.link;
                        return $http.get('/feed/banneri/' + response.data._id);
                    })
                    .catch((err) => {
                        console.log(err);
                    });
                $scope.copiedUrl = function () {
                    toastr.success('News url copied');
                }

                $scope.incrementHeaderBanner = function(id) {
                    $http.get('/feed/bannerwc/' + id);
                };

//                $http.get('/feed/banner/header')
//                    .then((response) => {
//                        $scope.headerBanner = response.data;
//                        $http.get('/feed/banneri/' + $scope.headerBanner._id);
//                    })
//                    .catch((err) => {
//                        console.log(err);
//                    });
                $scope.incrementClick = function() {
                    $http.get('/feed/bannerwc/' + $scope.bannerId);
                };
                $scope.addCount = function (id) {
                    $http.get('/feed/impressioninc/' + id)
                        .then(() => {

                        })
                        .catch((err) => {
                            console.log(err);
                        })
                };
                $scope.addCountClick = function (id, type) {
                    if(type) {
                        return $scope.addCountClickPR(id);
                    }
                    $http.get('/feed/clickfeedw/' + id)
                        .then(() => {

                        })
                        .catch((err) => {
                            console.log(err);
                        })
                };
                $scope.addCountPR = function(id) {
                    $http.get('/feed/pressreleasei/' +id)
                        .catch((err) => {
                            console.log(err);
                        })
                };

                $scope.addCountClickPR = function(id) {
                    $http.get('/feed/pressreleasec/' +id)
                        .catch((err) => {
                            console.log(err);
                        })
                };
                $scope.addCount($scope.newsDatas[0]._id);
                setTimeout(() => {
                    $scope.newsDatas.forEach((data,index) => {
                        $scope.dorpDownJs('share-button-' + index);
                    })
                },2000);
                $scope.getNews = function(isRefreshed) {
                    $scope.isNewsLoading = true;
                    $scope.page = 1;
                    var url = '/feed/web/feednews/' + $scope.page;
                    if($scope.Types.newsType === 'top') {
                        url = '/feed/web/topnews/' + $scope.page;
                    }
                    $http.post(url,{
                        isMobile: $scope.mobileDevice
                    })
                        .then(function(response) {
                            if(isRefreshed){
                                $scope.newsDatas = [];
                            }
                            $scope.page++;
                            $scope.newses = response.data;
                            $scope.newses.forEach(function(news) {
                                news.timeDiff =  $scope.getTimeDifference(news.toBePublishedOn);
                            });
                            if($scope.mobileDevice) {
                                $scope.newses.forEach(function (news) {
                                    if(!$scope.newsDatas[0] || news._id!==$scope.newsDatas[0]._id) {
                                        $scope.addCount(news._id);
                                        $scope.newsDatas.push(news);
                                    }
                                });
                                if(!$scope.isScrolledInitiated ) {
                                    $('#right-column').on('scroll', function() {
                                        $scope.isScrolledInitiated = true;
                                        if($(this).scrollTop() + $(this).innerHeight()+200 >= $(this)[0].scrollHeight) {
                                            if(!$scope.loading){
                                                $scope.loading = true;
                                                $scope.loadMore();//call function to load content when scroll reachs DIV bottom
                                            }
                                        }
                                    });
                                }
                            }
                            setTimeout(() => {
                                $scope.newsDatas.forEach((data,index) => {
                                    $scope.dorpDownJs('share-button-' + index);
                                })
                            },2000);
                        })
                        .catch(function(err) {
                            console.log(err);
                        });
                };


                $scope.getCoinDetails = function(){
                    $scope.loadingCoins = true;
                    $http.get('/coinmarket/coinslist')
                        .then(function(resposne) {
                            $scope.loadingCoins = false;
                            $scope.coinsDetails = resposne.data;
                        })
                        .catch(function(err) {
                            $scope.loadingCoins = false;
                            console.log(err);
                        });
                };
                $scope.getCoinDetails();
                $scope.getNews();

                $scope.refreshNow = function() {
                    $scope.getCoinDetails();
                    $scope.getNews(true);
                    $('#right-column').scrollTop(0);
                    $('#newslist-container').scrollTop(0);
                };
                $scope.selectNews = function(news) {
                    $scope.selectedNews = news;
                    let title = news.newsUrl;
                    $state.go('home.news', {newstitle:title});
                };
                $scope.goToMarket = function() {
                    window.location.href='/market';
                }
                $scope.goToAbout = function() {
                    window.location.href='/about';
                }

                $scope.searchNews = function ()
                {
                    if(!$scope.newsData.searchData || !$scope.newsData.searchData.length) {
                        $scope.page = 1;
                        $scope.newses =[];
                        $scope.loading = true;
                        $scope.loadMore();
                        return;
                    }
                    $http.get('/feed/search?keyword=' + $scope.newsData.searchData)
                        .then(function(response) {
                            $scope.newses = response.data;
                            $scope.newses.forEach(function(news) {
                                news.timeDiff =  $scope.getTimeDifference(news.toBePublishedOn);
                            });
                            if($scope.mobileDevice) {
                                $scope.newsDatas = angular.copy($scope.newses);
                            }
                        })
                        .catch(function(err) {
                            console.log(err);
                        })
                };
                $scope.getTimeDifference = function(timeStamp) {
                    let timeDiff = moment().diff(moment(timeStamp),'minutes');
                    if (timeDiff<0) {
                        timeDiff = timeDiff * -1;
                        if(timeDiff<=60) {
                            timeDiff = timeDiff + 'minutes';
                        } else if(timeDiff > 60 && timeDiff< 1440) {
                            timeDiff = (timeDiff/60).toFixed(0) + ' Hours';
                        } else {
                            timeDiff = (timeDiff/1440).toFixed(0) + ' Days';
                        }
                        timeDiff = 'will be published in ' + timeDiff ;
                    } else if(timeDiff<=60) {
                        timeDiff = timeDiff + ' minutes';
                    } else if(timeDiff > 60 && timeDiff< 1440) {
                        timeDiff = (timeDiff/60).toFixed(0) + ' Hours';
                    } else {
                        timeDiff = (timeDiff/1440).toFixed(0) + ' Days';
                    }
                    return timeDiff;
                };
                $scope.newsDatas[0].timeDiff =$scope.getTimeDifference($scope.newsDatas[0].toBePublishedOn);
                $('#newslist-container').on('scroll', function() {
                    if($(this).scrollTop() + $(this).innerHeight()+150 >= $(this)[0].scrollHeight) {
                        if(!$scope.loading){
                            $scope.loading = true;
                            $scope.loadMore();//call function to load content when scroll reachs DIV bottom
                        }
                    }
                });
                $scope.dorpDownJs = function(id) {
                    new needShareDropdown(document.getElementById(id));
                };
                $scope.loadMore = function() {
                    let url = '/feed/web/feednews/' + $scope.page;
                    if($scope.Types.newsType === 'top') {
                        url = '/feed/web/topnews/' + $scope.page;
                    }
                    $scope.loading = true;
                    $http.post(url,{
                        isMobile: $scope.mobileDevice
                    })
                        .then(function(response) {
                            $scope.page++;
                            response.data.forEach(function(news) {
                                news.timeDiff =  $scope.getTimeDifference(news.toBePublishedOn);
                            });
                            $scope.newses = $scope.newses.concat(response.data);
                            $scope.loading = false;
                            if($scope.mobileDevice) {
                                $scope.newsDatas.splice(1);
                                $scope.newses.forEach(function (news) {
                                    if(news._id!==$scope.newsDatas[0]._id) {
                                        $scope.newsDatas.push(news);
                                    }
                                });
                            }
                            setTimeout(() => {
                                $scope.newsDatas.forEach((data,index) => {
                                    $scope.dorpDownJs('share-button-' + index);
                                })
                            },2500);
                        })
                        .catch(function(err) {
                            $scope.loading = false;
                            console.log(err);
                        });
                };
                setInterval(function() {
                    $http.post('/feed/getlatest',{latestNews: $scope.newses[0].toBePublishedOn})
                        .then((response) => {
                            response.data.forEach(function(news) {
                                news.timeDiff =  $scope.getTimeDifference(news.toBePublishedOn);
                            });
                            $scope.newses = response.data.concat($scope.newses);
                        })
                        .catch((err) => {
                            console.log(err);
                        })
                },10000);

                $scope.newsChange = function() {
                    $scope.page = 1;
                    $scope.newses =[];
                    $scope.loadMore();
                };
                $scope.selectNews = function(news) {
                    $scope.newsDatas = [];
                    $scope.newsDatas.push(news);
                    $scope.newsDatas[0].imageUrls = typeof news.imageUrls === "object"?news.imageUrls:[news.imageUrls];
                    $location.path('/news/'+news.newsUrl);
                    if($scope.newsDatas[0].Type) {
                        $scope.addCountPR($scope.newsDatas[0]._id);
                    } else {
                        $scope.addCount($scope.newsDatas[0]._id);
                    }
                    setTimeout(() => {
                        $scope.dorpDownJs('share-button-0');
                    },1000);

                };
            })
    </script>
</head>
