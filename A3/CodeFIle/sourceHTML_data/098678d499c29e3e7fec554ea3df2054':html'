        <!-- Main Content -->
        <div id="home">
            <div class="home-header">

                <div class="hr-divider mt-1 mt-lg-3 mb-3">
                    <h3 class="hr-divider-content hr-divider-heading">
                        <button id="filter-btn" ng-click="showFilter = !showFilter" data-toggle="collapse" href="#collapseFilters" role="button"
